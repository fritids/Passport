/* 149 */ ;(function($){
/* 150 */ 	$.fn.fixedScroll = function(opts){

/* tito-base.js */

/* 151 */ 	
/* 152 */ 		var defaults = {fixedClass:'fixed', topLimit:0, bottomLimit:-1, absClass:'absolute', offset:0};
/* 153 */ 		var opts = $.extend(defaults, opts);
/* 154 */ 		
/* 155 */ 		return this.each(function(){
/* 156 */ 			var item = $(this);
/* 157 */ 			var over;
/* 158 */ 			$(window).bind('scroll resize', function(){
/* 159 */ 				var wt = $(window).scrollTop();
/* 160 */ 				var gt = 0;
/* 161 */ 				if (item.data('oy')){
/* 162 */ 					gt = item.data('oy');
/* 163 */ 				} else {
/* 164 */ 					gt = item.offset().top;
/* 165 */ 					item.data('oy', gt);
/* 166 */ 					item.data('ow', item.width());
/* 167 */ 				}	
/* 168 */ 				var dist = gt - wt;
/* 169 */ 				item.css('top', '');
/* 170 */ 				if (dist < opts.topLimit){
/* 171 */ 					item.addClass(opts.fixedClass);
/* 172 */ 					item.removeClass(opts.absClass);
/* 173 */ 					item.css('top', opts.topLimit);
/* 174 */ 					item.width(item.data('ow'));
/* 175 */ 				} else {
/* 176 */ 					item.removeClass(opts.fixedClass);
/* 177 */ 					item.removeClass(opts.absClass);
/* 178 */ 				}
/* 179 */ 				trace(dist);
/* 180 */ 				trace('ht = ' + item.outerHeight());
/* 181 */ 				if (opts.bottomLimit > -1 ){
/* 182 */ 					if (wt > opts.bottomLimit - item.outerHeight()){
/* 183 */ 						if (!over){
/* 184 */ 							over = wt;
/* 185 */ 						}
/* 186 */ 						item.removeClass(opts.fixedClass);
/* 187 */ 						item.addClass(opts.absClass);
/* 188 */ 						var top = over - opts.offset;
/* 189 */ 						trace('top = ' + top);
/* 190 */ 						item.css('top', top);
/* 191 */ 					}
/* 192 */ 				}
/* 193 */ 				
/* 194 */ 			
/* 195 */ 			});
