/* 280 */ function getGraphUnit(maxVal){
/* 281 */ 	var maxh = toNumber($('.graph-container').css('max-height'));
/* 282 */ 	var unit = maxh / maxVal;
/* 283 */ 	return unit;
/* 284 */ }
