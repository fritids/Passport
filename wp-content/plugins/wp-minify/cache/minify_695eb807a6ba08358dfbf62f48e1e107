/* 67  */ 	function selectTabFromGoal(goal){
/* 68  */ 		var tabs = $(".enter-goal-toggle").data('tabs');
/* 69  */ 		$('.enter-goal-toggle a, .enter-goal-toggle li').removeClass('active');
/* 70  */ 		if (goal == 'house' || goal == 'college' || goal == 'trip' || goal ==  'car' || goal == 'credit-card' || goal == 'retirement'){
/* 71  */ 			tabs.click(0);
/* 72  */ 			$('.enter-goal-toggle li').eq(0).addClass('active');
/* 73  */ 		} else if(goal == 'default'){
/* 74  */ 			var index = tabs.getIndex();
/* 75  */ 			$('.enter-goal-toggle li').eq(index).addClass('active');
/* 76  */ 			tabs.click(index);
/* 77  */ 		} else {
/* 78  */ 			try {
/* 79  */ 				tabs.click(0);
/* 80  */ 				tabs.next();
/* 81  */ 			} catch(e) {
/* 82  */ 				trace('tabs not loaded yet');
/* 83  */ 			}
/* 84  */ 			$('.enter-goal-toggle li').eq(1).addClass('active');
/* 85  */ 		}
/* 86  */ 	}
/* 87  */ 	
/* 88  */ 	function onGoalSelect(e){
/* 89  */ 		var val = $(this).val();
/* 90  */ 		if (!val){
/* 91  */ 			return;
/* 92  */ 		}
/* 93  */ 		val = val.toLowerCase();
/* 94  */ 		selectTabFromGoal(val);
/* 95  */ 		if (val == 'credit-card'){
/* 96  */ 			$('#cys-interest-rate-group').fadeIn();
/* 97  */ 			$("#cys-user-interest-rate").val(12);
/* 98  */ 			shebangSaveValue('cys-user-interest-rate', 12);
/* 99  */ 		} else if (val == 'retirement'){
/* 100 */ 			$("#cys-user-interest-rate").val(-4.6);

/* planner-control-your-spending.js */

/* 101 */ 			shebangSaveValue('cys-user-interest-rate', -4.6);
/* 102 */ 			$('#cys-interest-rate-group').fadeOut();
/* 103 */ 		} else {
/* 104 */ 			$("#cys-user-interest-rate").val(0);
/* 105 */ 			shebangSaveValue('cys-user-interest-rate', 0);
/* 106 */ 			$('#cys-interest-rate-group').fadeOut();
/* 107 */ 		}
/* 108 */ 		if (val == 'custom'){
/* 109 */ 			$('#custom-goal-form').show();
/* 110 */ 			$('#enter-goal-form').hide();
/* 111 */ 		} else {
/* 112 */ 			$('#enter-goal-form').show();
/* 113 */ 			$('#custom-goal-form').hide();
/* 114 */ 		}
/* 115 */ 		onOverallReady();
/* 116 */ 		shebangSaveValue('cys_goal_slug', val);
/* 117 */ 		var goalName = $(this).find('option[selected="selected"]').text();
/* 118 */ 		shebangSaveValue('cys_goal_name', goalName);
/* 119 */ 	}	
