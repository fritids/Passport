/* 76  */ 
/* 77  */ /**
/* 78  *|  * Cookie plugin
/* 79  *|  *
/* 80  *|  * Copyright (c) 2006 Klaus Hartl (stilbuero.de)
/* 81  *|  * Dual licensed under the MIT and GPL licenses:
/* 82  *|  * http://www.opensource.org/licenses/mit-license.php
/* 83  *|  * http://www.gnu.org/licenses/gpl.html
/* 84  *|  *
/* 85  *|  */
/* 86  */ 
/* 87  */ /**
/* 88  *|  * Create a cookie with the given name and value and other optional parameters.
/* 89  *|  *
/* 90  *|  * @example $.cookie('the_cookie', 'the_value');
/* 91  *|  * @desc Set the value of a cookie.
/* 92  *|  * @example $.cookie('the_cookie', 'the_value', { expires: 7, path: '/', domain: 'jquery.com', secure: true });
/* 93  *|  * @desc Create a cookie with all available options.
/* 94  *|  * @example $.cookie('the_cookie', 'the_value');
/* 95  *|  * @desc Create a session cookie.
/* 96  *|  * @example $.cookie('the_cookie', null);
/* 97  *|  * @desc Delete a cookie by passing null as value. Keep in mind that you have to use the same path and domain
/* 98  *|  *       used when the cookie was set.
/* 99  *|  *
/* 100 *|  * @param String name The name of the cookie.

/* jquery.total-storage.js */

/* 101 *|  * @param String value The value of the cookie.
/* 102 *|  * @param Object options An object literal containing key/value pairs to provide optional cookie attributes.
/* 103 *|  * @option Number|Date expires Either an integer specifying the expiration date from now on in days or a Date object.
/* 104 *|  *                             If a negative value is specified (e.g. a date in the past), the cookie will be deleted.
/* 105 *|  *                             If set to null or omitted, the cookie will be a session cookie and will not be retained
/* 106 *|  *                             when the the browser exits.
/* 107 *|  * @option String path The value of the path atribute of the cookie (default: path of page that created the cookie).
/* 108 *|  * @option String domain The value of the domain attribute of the cookie (default: domain of page that created the cookie).
/* 109 *|  * @option Boolean secure If true, the secure attribute of the cookie will be set and the cookie transmission will
/* 110 *|  *                        require a secure protocol (like HTTPS).
/* 111 *|  * @type undefined
/* 112 *|  *
/* 113 *|  * @name $.cookie
/* 114 *|  * @cat Plugins/Cookie
/* 115 *|  * @author Klaus Hartl/klaus.hartl@stilbuero.de
/* 116 *|  */
/* 117 */ 
/* 118 */ /**
/* 119 *|  * Get the value of a cookie with the given name.
/* 120 *|  *
/* 121 *|  * @example $.cookie('the_cookie');
/* 122 *|  * @desc Get the value of a cookie.
/* 123 *|  *
/* 124 *|  * @param String name The name of the cookie.
/* 125 *|  * @return The value of the cookie.
/* 126 *|  * @type String
/* 127 *|  *
/* 128 *|  * @name $.cookie
/* 129 *|  * @cat Plugins/Cookie
/* 130 *|  * @author Klaus Hartl/klaus.hartl@stilbuero.de
/* 131 *|  */
/* 132 */ jQuery.cookie = function(name, value, options) {
/* 133 */     if (typeof value != 'undefined') { // name and value given, set cookie
/* 134 */         options = options || {};
/* 135 */         if (value === null) {
/* 136 */             value = '';
/* 137 */             options.expires = -1;
/* 138 */         }
/* 139 */         var expires = '';
/* 140 */         if (options.expires && (typeof options.expires == 'number' || options.expires.toUTCString)) {
/* 141 */             var date;
/* 142 */             if (typeof options.expires == 'number') {
/* 143 */                 date = new Date();
/* 144 */                 date.setTime(date.getTime() + (options.expires * 24 * 60 * 60 * 1000));
/* 145 */             } else {
/* 146 */                 date = options.expires;
/* 147 */             }
/* 148 */             expires = '; expires=' + date.toUTCString(); // use expires attribute, max-age is not supported by IE
/* 149 */         }
/* 150 */         // CAUTION: Needed to parenthesize options.path and options.domain

/* jquery.total-storage.js */

/* 151 */         // in the following expressions, otherwise they evaluate to undefined
/* 152 */         // in the packed version for some reason...
/* 153 */         var path = options.path ? '; path=' + (options.path) : '';
/* 154 */         var domain = options.domain ? '; domain=' + (options.domain) : '';
/* 155 */         var secure = options.secure ? '; secure' : '';
/* 156 */         document.cookie = [name, '=', encodeURIComponent(value), expires, path, domain, secure].join('');
/* 157 */     } else { // only name given, get cookie
/* 158 */         var cookieValue = null;
/* 159 */         if (document.cookie && document.cookie != '') {
/* 160 */             var cookies = document.cookie.split(';');
/* 161 */             for (var i = 0; i < cookies.length; i++) {
/* 162 */                 var cookie = jQuery.trim(cookies[i]);
/* 163 */                 // Does this cookie string begin with the name we want?
/* 164 */                 if (cookie.substring(0, name.length + 1) == (name + '=')) {
/* 165 */                     cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
/* 166 */                     break;
/* 167 */                 }
/* 168 */             }
/* 169 */         }
/* 170 */         return cookieValue;
/* 171 */     }
/* 172 */ };

;
/* jquery.fixed-scroll.js */

/* 1  */ ;(function($){
/* 2  */ 	$.fn.fixedScroll = function(opts){
/* 3  */ 	
/* 4  */ 		var defaults = {fixedClass:'fixed', absClass:'absoute', container:'parent', offset:0};
/* 5  */ 		var opts = $.extend(defaults, opts);
/* 6  */ 		
/* 7  */ 		return this.each(function(){
/* 8  */ 			var item = $(this);
/* 9  */ 			var over;
/* 10 */ 			var container = item.parent();
/* 11 */ 			if (opts.container != 'parent'){
/* 12 */ 				container = opts.container;
/* 13 */ 			}
/* 14 */ 			var topLimit = item.offset().top - container.offset().top + opts.offset;
/* 15 */ 			$(window).bind('scroll resize', onScrollCheck);
/* 16 */ 			container.bind('resize', onScrollCheck);
/* 17 */ 			
/* 18 */ 			function onScrollCheck(){
/* 19 */ 				var wt = $(window).scrollTop();
/* 20 */ 				var gt = 0;
/* 21 */ 				if (item.data('oy')){
/* 22 */ 					gt = item.data('oy');
/* 23 */ 				} else {
/* 24 */ 					gt = item.offset().top;
/* 25 */ 					item.data('oy', gt);
/* 26 */ 				}	
/* 27 */ 				var dist = gt - wt;
/* 28 */ 				if (dist < topLimit){
/* 29 */ 					item.addClass(opts.fixedClass);
/* 30 */ 					item.removeClass(opts.absClass);
/* 31 */ 				} else {
/* 32 */ 					item.removeClass(opts.fixedClass);
/* 33 */ 					item.removeClass(opts.absClass);
/* 34 */ 				}
/* 35 */ 				if (wt > container.offset().top + container.height() - item.outerHeight()){
/* 36 */ 					item.removeClass(opts.fixedClass);
/* 37 */ 					item.addClass(opts.absClass);
/* 38 */ 					var top = container.height() - item.outerHeight();
/* 39 */ 					item.css('top', top);
/* 40 */ 				} else {
/* 41 */ 					item.css('top', '');
/* 42 */ 				}
/* 43 */ 			}
/* 44 */ 		});
/* 45 */ 	}
/* 46 */ })(jQuery);

