/* 51  */ 		function addGalleryOptions(){
/* 52  */ 			var powerTools = $('<span>Tools: <a href="#" id="autonumber">Auto-Number</a> | <a href="#" id="gal-showall">Show All</a> | <a href="#" id="gal-hideall">Hide All</a></span>');
/* 53  */ 			$('#sort-buttons').prepend(powerTools);
/* 54  */ 			$('#autonumber').click(autoNumberGallery);
