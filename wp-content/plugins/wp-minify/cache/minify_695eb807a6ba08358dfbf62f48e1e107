/* 407 */ 	function initActionDrawer(){
/* 408 */ 		$('#action-menu a.dropdown-trigger').click(toggleDropDown);
/* 409 */ 		$('.todo-no-items').show();
/* 410 */ 		$('.todo-move, .todo-changes').each(function(){
/* 411 */ 			var sec = $(this);
/* 412 */ 			sec.hide();
/* 413 */ 			if (sec.find('td').length){
/* 414 */ 				sec.show();
/* 415 */ 				$('.todo-no-items').hide();
/* 416 */ 			}
/* 417 */ 		});
/* 418 */ 	}	
/* 419 */ 	
/* 420 */ 	function initActionPlan(){
/* 421 */ 		$('.checkoff-todo input:checkbox').change(function(e){
/* 422 */ 			var curretPct = $('.action-prog-bar-leader').width() / $('.action-prog-bar').width() * 100;
/* 423 */ 			var rem = 100 - curretPct;
/* 424 */ 			var cb = $(this);
/* 425 */ 			var pctPerItem = rem / $('.todo-type:visible').length;
/* 426 */ 			var newPct = curretPct + pctPerItem;
/* 427 */ 			if (cb.is(':checked')){
/* 428 */ 				cb.parents('tr').find('.todo-type').addClass('checked').removeClass('unchecked');
/* 429 */ 				update_todo(cb.data('id'), 'checked');
/* 430 */ 			} else {
/* 431 */ 				cb.parents('tr').find('.todo-type').removeClass('checked').addClass('unchecked');
/* 432 */ 				update_todo(cb.data('id'), 'new');
/* 433 */ 				newPct = curretPct - pctPerItem;
/* 434 */ 			}
/* 435 */ 			setActionPlanProgressToPct(newPct);
/* 436 */ 			shebangSaveValue('cys_current_pct', Math.round(newPct));
/* 437 */ 			goNextActionItem();
/* 438 */ 		});
/* 439 */ 		var cp = toNumber($('.pct-complete-number').text().replace('%', ''));
/* 440 */ 		setActionPlanProgressToPct(cp);
/* 441 */ 	}
/* 442 */ 	
/* 443 */ 	function goNextActionItem(){
/* 444 */ 		var nextUp = $('.todo-container .todo-table .todo-type.unchecked').first().text().trim();
/* 445 */ 		if (!nextUp){
/* 446 */ 			nextUp = "You've completed all your Action Plan items";
/* 447 */ 		}
/* 448 */ 		$('.action-progress .next-step').html('<span class="next-step-label">Next step:</span> '+nextUp);
/* 449 */ 	}	
/* 450 */ 	

/* planner-control-your-spending.js */

