/* 419 */ function shebangInstantSave(){
/* 420 */ 	var input = $(this);
/* 421 */ 	var field = input.attr('data-field');
/* 422 */ 	var id = input.attr('id');
/* 423 */ 	if (!field){
/* 424 */ 		field = id;
/* 425 */ 	}
/* 426 */ 	var val = input.val();
/* 427 */ 	if (input.is(':checkbox')){
/* 428 */ 		if (input.is(':checked')){
/* 429 */ 			val = 'on';
/* 430 */ 		} else {
/* 431 */ 			val = 0;
/* 432 */ 		}
/* 433 */ 	}
/* 434 */ 	$.totalStorage(field, val);
/* 435 */ }
/* 436 */ 
/* 437 */ function shebangGetValue(field){
/* 438 */ 	return $.totalStorage(field);
/* 439 */ }
/* 440 */ 
/* 441 */ function shebangSetValue(field, val){
/* 442 */ 	return shebangSaveValue(field, val);
/* 443 */ }
/* 444 */ 
/* 445 */ function shebangSaveValue(field, val){
/* 446 */ 	return $.totalStorage($.trim(field), val);
/* 447 */ }
/* 448 */ 
/* 449 */ function roundTo(num, place){
/* 450 */ 	var nn = Math.round(num/place);

/* shebang.js */

/* 451 */ 	return nn * place;
/* 452 */ }
/* 453 */ 
/* 454 */ function addCommas(nStr){
/* 455 */ 	nStr += '';
/* 456 */ 	x = nStr.split('.');
/* 457 */ 	x1 = x[0];
/* 458 */ 	x2 = x.length > 1 ? '.' + x[1] : '';
/* 459 */ 	var rgx = /(\d+)(\d{3})/;
/* 460 */ 	while (rgx.test(x1)) {
/* 461 */ 		x1 = x1.replace(rgx, '$1' + ',' + '$2');
/* 462 */ 	}
/* 463 */ 	return x1 + x2;
/* 464 */ }
/* 465 */ 
/* 466 */ function toNumber(val){
/* 467 */ 	var str = String(val);
/* 468 */ 	str = str.replace(/[^\d.-]/g, "");
/* 469 */ 	var num = Number(str);
/* 470 */ 	return num;
/* 471 */ }
/* 472 */ 
/* 473 */ function toLower(str){
/* 474 */ 	if (str){
/* 475 */ 		return str.toLowerCase();
/* 476 */ 	}
/* 477 */ 	return str;
/* 478 */ }
/* 479 */ 
/* 480 */ function toBoolean(val){
/* 481 */ 	if (val == 0 || val == false || val == '0' || val == 'false' || val == ''){
/* 482 */ 		return false;
/* 483 */ 	} else {
/* 484 */ 		return true;
/* 485 */ 	}
/* 486 */ }
