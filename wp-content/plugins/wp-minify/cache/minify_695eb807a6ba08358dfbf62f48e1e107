/* 103 */ 	function setDefaultsToLocalStorage(){
/* 104 */ 		if (!$('#shebang_defaults').length){
/* 105 */ 			return;
/* 106 */ 		}
/* 107 */ 		var json = $('#shebang_defaults').text();
/* 108 */ 		if (!json){
/* 109 */ 			return;
/* 110 */ 		}
/* 111 */ 		var def = JSON.parse(json);
/* 112 */ 		for (var i = 0; i<def.length; i++){
/* 113 */ 			var row = def[i];
/* 114 */ 			var val = shebangGetValue(row.c_key);
/* 115 */ 			if (val == null){
/* 116 */ 				shebangSaveValue(row.c_key, row.c_value);
/* 117 */ 			}
/* 118 */ 		}
/* 119 */ 	}
/* 120 */ 	
/* 121 */ 	function setBooleanDisplayFromLocalStorage(){
/* 122 */ 		//for conditional display
/* 123 */ 		$('.ls-boolean-display').each(function(){
/* 124 */ 			var elm = $(this);
/* 125 */ 			elm.hide();
/* 126 */ 			var field = elm.attr('data-field');
/* 127 */ 			if (shebangGetValue(field)){
/* 128 */ 				elm.show();
/* 129 */ 			}
/* 130 */ 		});
/* 131 */ 		
/* 132 */ 		$('.ls-boolean-remove').each(function(){
/* 133 */ 			var elm = $(this);
/* 134 */ 			elm.hide();
/* 135 */ 			var field = elm.attr('data-field');
/* 136 */ 			if (shebangGetValue(field)){
/* 137 */ 				elm.show();
/* 138 */ 			} else {
/* 139 */ 				elm.remove();
/* 140 */ 			}
/* 141 */ 		});
/* 142 */ 		
/* 143 */ 	}
/* 144 */ 	
/* 145 */ 	function setValuesFromTotalStorage(){
/* 146 */ 		$('.ls-value').each(function(){
/* 147 */ 			var elm = $(this);
/* 148 */ 			
/* 149 */ 			var field = elm.attr('data-field');
/* 150 */ 			if (!field){

/* shebang.js */

/* 151 */ 				field = elm.attr('id');
/* 152 */ 			}
/* 153 */ 			var val = $.totalStorage(field);
/* 154 */ 			if (val == null){
/* 155 */ 				var fallback = elm.data('field-fallback');
/* 156 */ 				val = $.totalStorage(fallback);
/* 157 */ 			}
/* 158 */ 			if (!val && val != 0){
/* 159 */ 				trace(val + ' return on ' + field);
/* 160 */ 				return;
/* 161 */ 			}
/* 162 */ 			if (elm.is(':radio')){
/* 163 */ 				$('input[data-fiedl="'+field+'"]').removeAttr('checked');
/* 164 */ 				var sel = 'input[data-field="'+field+'"][value="'+val+'"]';
/* 165 */ 				var obj = $(sel);
/* 166 */ 				if (!obj.length){
/* 167 */ 					sel = 'input[id="'+field+'"][value="'+val+'"]';
/* 168 */ 					obj = $(sel);
/* 169 */ 				}
/* 170 */ 				obj.attr('checked', 'true');
/* 171 */ 			} else if (elm.is(':checkbox')){
/* 172 */ 				trace(field);
/* 173 */ 				if (val){
/* 174 */ 					elm.attr('checked', true);
/* 175 */ 				} else {
/* 176 */ 					elm.removeAttr('checked');
/* 177 */ 				}
/* 178 */ 			} else if (elm.is('input')){
/* 179 */ 				elm.val(val);
/* 180 */ 			} else if (elm.is('select')){
/* 181 */ 				elm.val(val);
/* 182 */ 				elm.find('option').removeAttr('selected')
/* 183 */ 				elm.find('option[value="'+val+'"]').attr('selected', 'selected');
/* 184 */ 			} else {
/* 185 */ 				if(toNumber(val) == val){
/* 186 */ 					if (elm.hasClass('round-to-10')){
/* 187 */ 						val = roundTo(val, 10);
/* 188 */ 					}
/* 189 */ 					if (!elm.hasClass('no-commas')){
/* 190 */ 						val = addCommas(val);
/* 191 */ 					}
/* 192 */ 					elm.html(val);
/* 193 */ 				} else {
/* 194 */ 					elm.html(val);
/* 195 */ 				}
/* 196 */ 			}
/* 197 */ 		});
/* 198 */ 	}
/* 199 */ 	
