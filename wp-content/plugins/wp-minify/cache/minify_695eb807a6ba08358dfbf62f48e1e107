/* 43  */ 		function pad(number, length) {
/* 44  */ 			var str = '' + number;
/* 45  */ 			while (str.length < length) {
/* 46  */ 				str = '0' + str;
/* 47  */ 			}
/* 48  */ 			return str;
/* 49  */ 		}
/* 50  */ 		

