/* 77  */ 		function warnIfNoFeaturedImage(){
/* 78  */ 			try {
/* 79  */ 				if (typenow == 'post'){
/* 80  */ 					$('input[type="submit"]').unbind('click');
/* 81  */ 					$('.post-php #publish, .post-new-php #publish').unbind();
/* 82  */ 					$('.post-php #publish, .post-new-php #publish').click(function(e){
/* 83  */ 						if (!$('.attachment-post-thumbnail').length){
/* 84  */ 							var override = confirm('Are you sure you want to publish without a photo?');
/* 85  */ 							if (!override){
/* 86  */ 								e.preventDefault();
/* 87  */ 								e.stopPropagation();
/* 88  */ 								setTimeout(function(){
/* 89  */ 									$('#publish').removeClass('button-primary-disabled');
/* 90  */ 								}, 500);
/* 91  */ 								
/* 92  */ 								$('.ajax-loading').hide();
/* 93  */ 							}
/* 94  */ 						}
/* 95  */ 					});
/* 96  */ 				}
/* 97  */ 			} catch (e){
/* 98  */ 				trace(e);
/* 99  */ 			}
/* 100 */ 		}

/* upstatement-admin.js */

/* 101 */ 		
/* 102 */ 		function addExpirationDate(){
/* 103 */ 			if ($('body').hasClass('post-new-php')){
/* 104 */ 				var url = String(window.location);
/* 105 */ 				if (url.indexOf('recipes') > 0){
/* 106 */ 					// on a recipe page
/* 107 */ 					var d = new Date();
/* 108 */ 					var y = d.getFullYear();
/* 109 */ 					var m = d.getMonth();
/* 110 */ 					var date = d.getDate();
/* 111 */ 					/* because JS is 0-based and the db is 1-based we need to increase by one to normalize  */
/* 112 */ 					d++;
/* 113 */ 					m++;
/* 114 */ 					if (m == 12){
/* 115 */ 						//curent month is dec so expires is jan
/* 116 */ 						y++;
/* 117 */ 						m = 1
/* 118 */ 					} else {
/* 119 */ 						m++;
/* 120 */ 					}
/* 121 */ 					
/* 122 */ 					var dateString = y+'-'+pad(m,2) +'-'+pad(date, 2);
/* 123 */ 					trace('dateString = ' + dateString);
/* 124 */ 					$('#expires').val(dateString);
/* 125 */ 				}
/* 126 */ 			}
/* 127 */ 		}
/* 128 */ 		
/* 129 */ 	});
/* 130 */ 	
/* 131 */ 	function trace(s) {
/* 132 */ 	  try { console.log(s) } catch (e) { }
/* 133 */ 	}
/* 134 */ })(jQuery);

;
