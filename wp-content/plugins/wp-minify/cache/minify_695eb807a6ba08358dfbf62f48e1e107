/* 297 */ function redrawBarGraph(maxVal){
/* 298 */ 	if (!maxVal){
/* 299 */ 		maxVal = getGraphMaxVal();
/* 300 */ 	}

/* shebang.js */

/* 301 */ 	$('.bar-graph-bar').each(function(){
/* 302 */ 		var htmlID = $(this).attr('id');
/* 303 */ 		var val = $(this).find('.amount').html();
/* 304 */ 		if (val == '' || val == null){
/* 305 */ 			return;
/* 306 */ 		}
/* 307 */ 		if (val){
/* 308 */ 			val = toNumber(val.replace('$', ''));
/* 309 */ 		}
/* 310 */ 		if (val){
/* 311 */ 			updateBarGraphBar(htmlID, val);
/* 312 */ 		} else {
/* 313 */ 			hideBarGraphBar(htmlID);
/* 314 */ 		}
/* 315 */ 	});
