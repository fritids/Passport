/* 468 */ 	function initEmailReminder(){
/* 469 */ 		$('.remind-button').click(function(e){
/* 470 */ 			e.preventDefault();
/* 471 */ 			var planner = $('body').data('post-parent-slug');
/* 472 */ 			var uemail = $('#remind-email-input').val();
/* 473 */ 			var ufreq = $('#remind-time-select').val();
/* 474 */ 			shebangSaveValue('cys_remind_email', uemail);
/* 475 */ 			shebangSaveValue('cys_remind_freq', ufreq);
/* 476 */ 			saveEmailData(uemail, ufreq, planner);
/* 477 */ 			showRemindComplete(uemail, ufreq, 500);
/* 478 */ 			$('.action-prog-bar-leader').css('width', '75%');
/* 479 */ 			$('.action-progress .pct-complete-number').text('75%'); 	
/* 480 */ 			goNextActionItem();
/* 481 */ 			
/* 482 */ 		});
/* 483 */ 		$('.change-remind-btn, .change-remind-link').click(function(e){
/* 484 */ 			e.preventDefault();
/* 485 */ 			$('.remind-complete-msg').fadeOut(300, function(){
/* 486 */ 				$('.remind-form').fadeIn(300);
/* 487 */ 			});
/* 488 */ 		});
/* 489 */ 		$('.cancel-remind-btn').click(function(e){
/* 490 */ 			$('#remind-email-input').val('');
/* 491 */ 		});
/* 492 */ 		
/* 493 */ 		if ($('#remind-email-input').val()){
/* 494 */ 			var freq = $('#remind-time-select').val();
/* 495 */ 			var email = shebangGetValue('cys_remind_email');
/* 496 */ 			showRemindComplete(email, freq, 1);
/* 497 */ 			goNextActionItem();
/* 498 */ 		}
/* 499 */ 	}	
