/* 501 */ 	function toggleDropDown(){
/* 502 */ 		var ul = $('#action-menu .action-list');
/* 503 */ 		//ul.css('top', $('#action-menu').position().top + $('#action-menu').outerHeight() + 5);
/* 504 */ 		if (ul.is(":visible")){
/* 505 */ 			ul.slideUp();
/* 506 */ 		} else {
/* 507 */ 			ul.slideDown();
/* 508 */ 		}
/* 509 */ 	}
/* 510 */ 	
/* 511 */ 	function initGoalAmount(){
/* 512 */ 		$('input[name=save]').blur(function(){
/* 513 */ 			updateGoalAmount($(this).val());			
/* 514 */ 		});
/* 515 */ 		$('input[name=save]').keyup(function(){
/* 516 */ 			updateGoalAmount($(this).val());
/* 517 */ 		});
/* 518 */ 		var saved = toNumber($('#savings-menu .value.saved').text());
/* 519 */ 		var goal = toNumber($('#savings-menu .value.goal').text());
/* 520 */ 		updateGoalBar(saved/goal);
/* 521 */ 	}
/* 522 */ 	
/* 523 */ 	function updateGoalAmount(amt){
/* 524 */ 		trace('updategoalamount');
/* 525 */ 		if (amt == 0){
/* 526 */ 			amt = '0';
/* 527 */ 		}
/* 528 */ 		amt = toNumber(amt);
/* 529 */ 		var goal = addCommas(amt);
/* 530 */ 		if (!goal){
/* 531 */ 			goal = '0';
/* 532 */ 		}
/* 533 */ 		$('.money-number .value.goal').text(goal);
/* 534 */ 		if (amt == 0){
/* 535 */ 			$('.money-number .value.goal').addClass('gray');
/* 536 */ 			$('.money-number .money-goal').hide();
/* 537 */ 			$('.money-number .slash').hide();
/* 538 */ 		} else {
/* 539 */ 			$('.money-number .value.goal').removeClass('gray');
/* 540 */ 			$('.money-number .money-goal').show();
/* 541 */ 			$('.money-number .slash').show();
/* 542 */ 		}	
/* 543 */ 		updateSavings();
/* 544 */ 	}
/* 545 */ 	
/* 546 */ 	function initSmallExpenses(){
/* 547 */ 		$('.trigger-cys-todo').live('click', onActionPlanItemSelect);
/* 548 */ 		$('.add-your-own-name').live('blur', function(){
/* 549 */ 			var input = $(this);
/* 550 */ 			var cb = input.parents('td').prev().find('input:checkbox');

/* planner-control-your-spending.js */

/* 551 */ 			cb.val(input.val());
/* 552 */ 			input.data('checkbox', cb);
/* 553 */ 		});
/* 554 */ 		
/* 555 */ 		$('.add-your-own-amount').live('blur', function(){
/* 556 */ 			var input = $(this);
/* 557 */ 			var cb = input.parents('td').parent().find('input:checkbox');
/* 558 */ 			cb.attr('data-amount', input.val());
/* 559 */ 			input.data('checkbox', cb);
/* 560 */ 		});
/* 561 */ 		$('.add-your-own-amount').numeric();
/* 562 */ 	}
/* 563 */ 	
/* 564 */ 	function onActionPlanItemSelect(e){
/* 565 */ 		var cb = $(this);
/* 566 */ 		var id = cb.data('id');
/* 567 */ 		var body = $('body');
/* 568 */ 		var tr = cb.parents('tr');
/* 569 */ 		var value = cb.attr('data-amount');
/* 570 */ 		var text = cb.val();
/* 571 */ 		
/* 572 */ 		if (cb.is(':checked')){	
/* 573 */ 			$('.cys-todo-id-'+id).attr('checked', 'true');
/* 574 */ 			var name = sendToActionPlan(cb, text, value, id, true);
/* 575 */ 			tr.find('.checkbox-label').addClass('green');
/* 576 */ 			save_todo(cb.val(), value, id, cb.data('parent'));
/* 577 */ 			updateSavings();
/* 578 */ 			if (body.hasClass('cut-spending-page')){
/* 579 */ 				trace('checking savings');
/* 580 */ 				setTimeout(checkSavingsAgainstGoal, 1500);
/* 581 */ 			} 
/* 582 */ 		} else {
/* 583 */ 			$('.cys-todo-id-'+id).removeAttr('checked');
/* 584 */ 			tr.find('.checkbox-label').removeClass('green');
/* 585 */ 			delete_todo(id);
/* 586 */ 			removeFromActionPlan(id);
/* 587 */ 			updateSavings();
/* 588 */ 		}
/* 589 */ 	}
/* 590 */ 	
/* 591 */ 	function updateSavings(){
/* 592 */ 		var val = getSavings();
/* 593 */ 		var goalAmt = getGoal();
/* 594 */ 		var pct = (val)/goalAmt;
/* 595 */ 		updateGoalBar(pct);
/* 596 */ 		$('.money-saved .money-number .value.saved').countTo({to:val, speed:400, format:basicNumberFormat});
/* 597 */ 		shebangSaveValue('cys_savings', val);
/* 598 */ 	}
/* 599 */ 	
/* 600 */ 	function getGoal(){

/* planner-control-your-spending.js */

/* 601 */ 		var goalAmt = $('.money-number .value.goal').text();
/* 602 */ 		return toNumber(goalAmt);
/* 603 */ 	}
/* 604 */ 	
/* 605 */ 	function getSavings(){
/* 606 */ 		var tds = cysGetTodos();
/* 607 */ 		var savings = 0;
/* 608 */ 		for (var i = 0; i<tds.length; i++){
/* 609 */ 			savings += toNumber(tds[i].amount);
/* 610 */ 		}
/* 611 */ 		return savings;
/* 612 */ 	}
/* 613 */ 	
/* 614 */ 	function checkSavingsAgainstGoal(){
/* 615 */ 		var goal = getGoal();
/* 616 */ 		if (goal > 0){
/* 617 */ 			var saved = getSavings();
/* 618 */ 			if (saved >= goal){
/* 619 */ 				//user has hit goal
/* 620 */ 				showSavingsFlag(true)
/* 621 */ 			} else if (saved < goal){
/* 622 */ 				showSavingsFlag(false);
/* 623 */ 			} else {
/* 624 */ 				// I have no idea
/* 625 */ 			}
/* 626 */ 		}
/* 627 */ 	}
/* 628 */ 	
/* 629 */ 	var smUsed = false;
/* 630 */ 	
/* 631 */ 	function checkIfGoalIsAlreadyReached(){
/* 632 */ 		var goal = getGoal();
/* 633 */ 		if (goal > 0){
/* 634 */ 			var saved = getSavings();
/* 635 */ 			if (saved >= goal){
/* 636 */ 				smUsed = true;	
/* 637 */ 			}
/* 638 */ 		}
/* 639 */ 	}
/* 640 */ 	
/* 641 */ 	function showSavingsFlag(opt){
/* 642 */ 		if (opt){
/* 643 */ 			var sm = $('#spending-modal');
/* 644 */ 			if (smUsed){
/* 645 */ 				trace('already used');
/* 646 */ 				return;
/* 647 */ 			}
/* 648 */ 			$('#spending-modal').modal();
/* 649 */ 			smUsed = true;
/* 650 */ 			$('.close-modal, #simplemodal-overlay').click(closeModal);

/* planner-control-your-spending.js */

/* 651 */ 		}
/* 652 */ 	}
/* 653 */ 	
/* 654 */ 	function closeModal(){
/* 655 */ 		$.modal.close();
/* 656 */ 	}
/* 657 */ 	
/* 658 */ 	function updateGoalBar(pct){
/* 659 */ 		var w = $('.money-bar').width();
/* 660 */ 		if (pct > 1){
/* 661 */ 			pct = 1;
/* 662 */ 		}
/* 663 */ 		var tw = pct * w;
/* 664 */ 		var mbl = $('.money-bar-leader');
/* 665 */ 		mbl.css({width:tw});
/* 666 */ 	}	
/* 667 */ 		
/* 668 */ 	function sendToActionPlan(item, text, amt, id, animate){
/* 669 */ 		var tdid = id;
/* 670 */ 		var tr = $('.todo-section [data-tdid="'+tdid+'"]');
/* 671 */ 		if (tr.length){
/* 672 */ 			//just update the values
/* 673 */ 			tr.find('.amount').text(amt);
/* 674 */ 			tr.find('label').text(text);
/* 675 */ 			return;
/* 676 */ 		}
/* 677 */ 		var liString = '<tr class="row-3 action-drawer-item" data-tdid="'+tdid+'" data-id="'+id+'"><td class="todo-type unchecked first"><label for="tires" class="checkbox-label">'+text+'</label></td><td class="todo-savings last">';
/* 678 */ 		if (amt){
/* 679 */ 			liString += '<span class="dollar">$</span><span class="amount">'+amt+'</span>';
/* 680 */ 		}
/* 681 */ 		liString += '</td></tr>';
/* 682 */ 		var li = $(liString);
/* 683 */ 		$('.everyday-table .todo-table-body').append(li);
/* 684 */ 		var tr = item.closest('tr,div');
/* 685 */ 		if (animate){
/* 686 */ 			try {
/* 687 */ 				tr.sailTo($('#action-menu'), {duration:800, text:text, textTarget:'.action-menu-action-h1 .action-item'}); } catch(e){ trace(e); }
/* 688 */ 		}
/* 689 */ 		$('.todo-no-items').hide();
/* 690 */ 		checkActionDrawerSections();
/* 691 */ 		return text;
/* 692 */ 	}
/* 693 */ 	
/* 694 */ 	function checkActionDrawerSections(){
/* 695 */ 		$('.todo-table').each(function(){
/* 696 */ 			var table = $(this);
/* 697 */ 			if (table.find('td').length){
/* 698 */ 				table.show();
/* 699 */ 				table.closest('.todo-section').show();
/* 700 */ 			} else {

/* planner-control-your-spending.js */

/* 701 */ 				table.closest('.todo-section').hide();
/* 702 */ 			}	
/* 703 */ 		});
/* 704 */ 	}
/* 705 */ 	
/* 706 */ 	function removeFromActionPlan(id){
/* 707 */ 		$('tr[data-id="'+id+'"]').remove();
/* 708 */ 		checkActionDrawerSections();
/* 709 */ 	}	
/* 710 */ 	
/* 711 */ 	function cysGetTodos(){
/* 712 */ 		var cys_todos = $.totalStorage('cys_todos');
/* 713 */ 		if (!cys_todos){
/* 714 */ 			cys_todos = new Array();
/* 715 */ 		}
/* 716 */ 		var ids = new Array();
/* 717 */ 		var na = new Array();
/* 718 */ 		for (var i = 0; i<cys_todos.length; i++){
/* 719 */ 			var todo = cys_todos[i];
/* 720 */ 			if ($.inArray(todo.id, ids) == -1 && todo.value.length){
/* 721 */ 				ids.push(todo.id);
/* 722 */ 				na.push(todo);
/* 723 */ 			}
/* 724 */ 		}
/* 725 */ 		return na;
/* 726 */ 	}
/* 727 */ 	
/* 728 */ 	function getCheckedToDosForActionPlan(){
/* 729 */ 		if (!$('body').hasClass('make-a-plan')){
/* 730 */ 			trace('exit');
/* 731 */ 			return;
/* 732 */ 		}
/* 733 */ 		var tds = cysGetTodos();
/* 734 */ 		trace(tds);
/* 735 */ 		for (var i = 0; i<tds.length; i++){
/* 736 */ 			var td = tds[i];
/* 737 */ 			var tr = $('<tr class="checkoff-todo row-'+i+'"></tr>');
/* 738 */ 			var td1 = $('<td class="checkbox-container"><input type="checkbox" name="todo-'+td.id+'" id="todo-'+td.id+'" value="'+td.value+'" class="checkbox-input" data-id="'+td.id+'"/></td>');
/* 739 */ 			var td2 = $('<td class="todo-type unchecked"><label for="todo-'+td.id+'" class="checkbox-label">'+td.value+'</label></td>');
/* 740 */ 			var td3 = $('<td class="todo-savings"><span class="dollar">$</span>'+td.amount+'</td>');
/* 741 */ 			var td4 = $('<td class="todo-link"><a href="#" class="link-text">Help me do it &raquo;</a></td>');
/* 742 */ 			if (td.status == 'checked'){
/* 743 */ 				td2.removeClass('unchecked');
/* 744 */ 				td2.addClass('checked');
/* 745 */ 				td1.find('input:checkbox').attr('checked', 'checked');
/* 746 */ 			}
/* 747 */ 			tr.append(td1).append(td2).append(td3).append(td4);
/* 748 */ 			$('#cys-action-items').append(tr);
/* 749 */ 		}
/* 750 */ 		

/* planner-control-your-spending.js */

/* 751 */ 	}
/* 752 */ 		
/* 753 */ 	function getCheckedToDos(){
/* 754 */ 		trace('getCheckedToDos');
/* 755 */ 		var tds = cysGetTodos();
/* 756 */ 		for (var i = 0; i<tds.length; i++){
/* 757 */ 			var td = tds[i];
/* 758 */ 			var cbl = $('.trigger-cys-todo[data-id="'+td.id+'"]');
/* 759 */ 			var cb = cbl.closest('tr, div').find('input:checkbox');
/* 760 */ 			cb.attr('checked', 'true');
/* 761 */ 			cb.attr('data-amount', td.amount);
/* 762 */ 			cb.closest('tr').find('.add-your-own-amount').val(td.amount);
/* 763 */ 			sendToActionPlan(cb, td.value, td.amount, td.id, false);
/* 764 */ 			var id = String(td.id);
/* 765 */ 			var sel = $("."+id);
/* 766 */ 			if (id.indexOf('ayo') == 0 && !sel.length && td.value){
/* 767 */ 				//it's an add-your own
/* 768 */ 				
/* 769 */ 				var parent = td.parent;
/* 770 */ 				var tr = $('<tr class="add-todo spending-row add-your-own-tip '+id+'"><td class="checkbox-container first"><input type="checkbox" name="custom" value="'+td.value+'" class="checkbox-input trigger-cys-todo" data-parent="spending-cat-health-care" data-id="'+td.id+'" data-tdid="'+td.id+'" checked="checked" data-amount="'+td.amount+'"></td><td><input type="text" class="checkbox-label blank-input add-your-own-name" placeholder="Add your own" value="'+td.value+'"></td><td class="spending-savings last"><span class="dollar">$</span><input type="text" placeholder="0" class="spending-savings blank-input add-your-own-amount" value="'+td.amount+'"></td></tr>');
/* 771 */ 				$('.pane#'+parent).find('.table-body .add-your-own-tip').last().before(tr);
/* 772 */ 			}
/* 773 */ 		}
/* 774 */ 	}
/* 775 */ 	
/* 776 */ 	function save_todo(name, amount, id, parent){
/* 777 */ 		var cys_todos = cysGetTodos();
/* 778 */ 		var obj = {amount:amount, status:'new', value:name, id:id, parent:parent};
/* 779 */ 		cys_todos.push(obj);
/* 780 */ 		if (name){
/* 781 */ 			shebangSaveValue('cys_todos', cys_todos);
/* 782 */ 		}
/* 783 */ 	}
/* 784 */ 	
/* 785 */ 	function delete_todo(id){
/* 786 */ 		var cys_todos = cysGetTodos();
/* 787 */ 		var rows = new Array();
/* 788 */ 		for (var i = 0; i<cys_todos.length; i++){
/* 789 */ 			var td = cys_todos[i];
/* 790 */ 			if (td.id == id){
/* 791 */ 				rows.push(i);
/* 792 */ 			}
/* 793 */ 		}	
/* 794 */ 		if (rows.length){
/* 795 */ 			for (i = 0; i<rows.length; i++){
/* 796 */ 				cys_todos.splice(rows[i], 1);
/* 797 */ 			}
/* 798 */ 		}
/* 799 */ 		shebangSaveValue('cys_todos', cys_todos);
/* 800 */ 	}

/* planner-control-your-spending.js */

/* 801 */ 	
/* 802 */ 	function find_todo_index(id){
/* 803 */ 		var cys_todos = cysGetTodos();
/* 804 */ 		for (var i = 0; i<cys_todos.length; i++){
/* 805 */ 			var td = cys_todos[i];
/* 806 */ 			if (td.id == id){
/* 807 */ 				return i;
/* 808 */ 			}
/* 809 */ 		}
/* 810 */ 	}
/* 811 */ 	
/* 812 */ 	function update_todo_field(id, field, amount){
/* 813 */ 		var cys_todos = cysGetTodos();
/* 814 */ 		var i = find_todo_index(id);
/* 815 */ 		if (!i && i != 0){
/* 816 */ 			trace('no index found');
/* 817 */ 			return;
/* 818 */ 		}
/* 819 */ 		cys_todos[i][field] = amount;
/* 820 */ 		$('.action-drawer-item[id="'+id+'"]').find('.amount').html(amount);
/* 821 */ 		shebangSaveValue('cys_todos', cys_todos);
/* 822 */ 	}
/* 823 */ 	
/* 824 */ 	function update_todo(id, status){
/* 825 */ 		var cys_todos = cysGetTodos();
/* 826 */ 		var i = find_todo_index(id);
/* 827 */ 		cys_todos[i].status = status;
/* 828 */ 		shebangSaveValue('cys_todos', cys_todos);
/* 829 */ 	}	
/* 830 */ 	
/* 831 */ //})(jQuery);
