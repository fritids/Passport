/* jquery.count-to.js */

/* 1  */ (function($) {
/* 2  */     $.fn.countTo = function(options) {
/* 3  */         // merge the default plugin settings with the custom options
/* 4  */         options = $.extend({}, $.fn.countTo.defaults, options || {});
/* 5  */ 		var countLimit = 1000;
/* 6  */ 		var c = 0;
/* 7  */         // how many times to update the value, and how much to increment the value on each update
/* 8  */         if (options.from == 'cv'){
/* 9  */         	options.from = parseFloat($(this).text().replace('$', '').replace(',','')) * 1;
/* 10 */         }
/* 11 */         var loops = Math.ceil(options.speed / options.refreshInterval),
/* 12 */             increment = (options.to - options.from) / loops;
/* 13 */ 
/* 14 */         return $(this).each(function() {
/* 15 */             var _this = this,
/* 16 */                 loopCount = 0,
/* 17 */                 value = options.from,
/* 18 */                 interval = setInterval(updateTimer, options.refreshInterval);
/* 19 */ 			
/* 20 */ 		
/* 21 */             function updateTimer() {
/* 22 */             	c++;
/* 23 */             	if (c > countLimit){
/* 24 */             		return;
/* 25 */             	}
/* 26 */                 value += increment;
/* 27 */                 loopCount++;
/* 28 */                 var nt = value.toFixed(options.decimals);
/* 29 */ 				if (options.format && options.format != null){
/* 30 */ 					try {
/* 31 */ 						nt = $.formatNumber(nt, options.format);
/* 32 */ 					} catch(e){
/* 33 */ 						//trace('formatNumber not avaiable');
/* 34 */ 					}
/* 35 */ 				}
/* 36 */ 				if (nt == ''){
/* 37 */ 					nt = 0;
/* 38 */ 				}
/* 39 */                 $(_this).text(nt);
/* 40 */ 
/* 41 */                 if (typeof(options.onUpdate) == 'function') {
/* 42 */                     options.onUpdate.call(_this, value);
/* 43 */                 }
/* 44 */ 
/* 45 */                 if (loopCount >= loops) {
/* 46 */                     clearInterval(interval);
/* 47 */                     value = options.to;
/* 48 */ 
/* 49 */                     if (typeof(options.onComplete) == 'function') {
/* 50 */                         options.onComplete.call(_this, value);

/* jquery.count-to.js */

/* 51 */                     }
/* 52 */                 }
/* 53 */             }
/* 54 */         });
/* 55 */     };
/* 56 */ 
/* 57 */     $.fn.countTo.defaults = {
/* 58 */         from: 'cv',  // the number the element should start at
/* 59 */         to: 100,  // the number the element should end at
/* 60 */         speed: 800,  // how long it should take to count between the target numbers
/* 61 */         refreshInterval: 20,  // how often the element should be updated
/* 62 */         decimals: 0,  // the number of decimal places to show
/* 63 */         onUpdate: null,  // callback method for every time the element is updated,
/* 64 */         onComplete: null,  // callback method for when the element finishes updating
/* 65 */         format:null, //number format to use
/* 66 */     };
/* 67 */ })(jQuery);

