/* jquery.total-storage.js */

/* 1   */ ;(function($){
/* 2   */ 
/* 3   */ 	/* Variables I'll need throghout */
/* 4   */ 
/* 5   */ 	var ls;
/* 6   */ 	var supported = true;
/* 7   */ 	if (typeof localStorage == 'undefined' || typeof JSON == 'undefined') {
/* 8   */ 		supported = false;
/* 9   */ 	} else {
/* 10  */ 		ls = localStorage;
/* 11  */ 	}
/* 12  */ 	
/* 13  */ 	/* Make the methods public */
/* 14  */ 
/* 15  */ 	$.totalStorage = function(key, value, options){
/* 16  */ 		return $.totalStorage.impl.init(key, value);
/* 17  */ 	}
/* 18  */ 	
/* 19  */ 	$.totalStorage.setItem = function(key, value){
/* 20  */ 		return $.totalStorage.impl.setItem(key, value);
/* 21  */ 	}
/* 22  */ 	
/* 23  */ 	$.totalStorage.getItem = function(key){
/* 24  */ 		return $.totalStorage.impl.getItem(key);
/* 25  */ 	}
/* 26  */ 	
/* 27  */ 	/* Object to hold all methods: public and private */
/* 28  */ 	
/* 29  */ 	$.totalStorage.impl = {
/* 30  */ 		
/* 31  */ 		init: function(key, value){
/* 32  */ 			if (typeof value != 'undefined') {
/* 33  */ 				return this.setItem(key, value);	
/* 34  */ 			} else {
/* 35  */ 				return this.getItem(key);
/* 36  */ 			}
/* 37  */ 		},
/* 38  */ 		
/* 39  */ 		setItem: function(key, value){
/* 40  */ 			if (!supported){
/* 41  */ 				$.cookie(key, value);
/* 42  */ 				return true;
/* 43  */ 			}
/* 44  */ 			var saver = JSON.stringify(value);
/* 45  */ 			ls.setItem(key, saver);
/* 46  */ 			return saver;
/* 47  */ 		},
/* 48  */ 		
/* 49  */ 		getItem: function(key){
/* 50  */ 			if (!supported){

/* jquery.total-storage.js */

/* 51  */ 				return this.parseResult($.cookie(key));
/* 52  */  			}
/* 53  */ 			return this.parseResult(ls.getItem(key));
/* 54  */ 		},	
/* 55  */ 		
/* 56  */ 		parseResult: function(res){
/* 57  */ 			var ret;
/* 58  */ 			try {
/* 59  */ 				ret = JSON.parse(res);
/* 60  */ 				if (ret == 'true'){
/* 61  */ 					ret = true;
/* 62  */ 				}
/* 63  */ 				if (ret == 'false'){
/* 64  */ 					ret = false;
/* 65  */ 				}
/* 66  */ 				if (parseFloat(ret.replace(',', '')) == ret){
/* 67  */ 					ret = parseFloat(ret);
/* 68  */ 				}
/* 69  */ 			} catch(e){}
/* 70  */ 			return ret;
/* 71  */ 		}
/* 72  */ 	}
/* 73  */ 
/* 74  */ })(jQuery);
