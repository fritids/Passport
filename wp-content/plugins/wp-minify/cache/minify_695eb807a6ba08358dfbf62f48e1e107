/* 157 */ 		$('#overall-savings-input').returnKey(onOverallReady);
/* 158 */ 		
/* 159 */ 		$('#finish-month-select').bind('blur change click', onOverallReady);
/* 160 */ 		$('#finish-year-select').bind('blur change click', onOverallReady);
/* 161 */ 		
/* 162 */ 		$('#monthly-savings-input').bind('blur keyup click', onMonthlyReady);
/* 163 */ 		$('#monthly-savings-input').returnKey(onMonthlyReady);
/* 164 */ 		
/* 165 */ 		$('#goal-annual-input').bind('blur keyup click', onYearlyReady);
/* 166 */ 		$('#goal-annual-input').returnKey(onYearlyReady);
/* 167 */ 		
/* 168 */ 		var d = new Date();
/* 169 */ 		var cm = d.getMonth() + 1;
/* 170 */ 		var nextYear = (d.getFullYear()) + 1;
/* 171 */ 		if (toLower($('#finish-month-select').val()) == 'default'){
/* 172 */ 			$('#finish-month-select option[value="'+cm+'"]').attr('selected', 'selected');
/* 173 */ 		}
/* 174 */ 		if (toLower($('#finish-year-select').val()) == 'default'){
/* 175 */ 			$('#finish-year-select option[value="'+nextYear+'"]').attr('selected', 'selected');
/* 176 */ 		}
/* 177 */ 		$('#custom-goal-input').blur(onCustomGoalReady);
/* 178 */ 		
/* 179 */ 		$('#monthly-savings-input').trigger('blur');
/* 180 */ 		
/* 181 */ 		
/* 182 */ 		$('.enter-goal-toggle-item').click(function(){
/* 183 */ 			$('.enter-goal-toggle-item').removeClass('active');
/* 184 */ 			$(this).addClass('active');
/* 185 */ 			var ctab = $('.enter-goal-toggle-item').index($(this));
/* 186 */ 			shebangSaveValue('cys_goal_tab', ctab);
/* 187 */ 		});
/* 188 */ 		
/* 189 */ 		var ii = $('.enter-goal-toggle').attr('data-start-index');
/* 190 */ 		$(".enter-goal-toggle").tabs("#enter-goal-form > .pane",{
/* 191 */ 			current:'active', initialIndex:Number(ii),
/* 192 */ 		});	
/* 193 */ 		trace($(".enter-goal-toggle").data('tabs'));
/* 194 */ 		//$('.enter-goal-toggle').data('tabs').click(0);
/* 195 */ 		
/* 196 */ 		if ($('.todo-target .value.goal').text() == '0' || !$('.todo-target .value.goal').text()){
/* 197 */ 			$('.enter-goal-summary').hide();
/* 198 */ 		}
/* 199 */ 	}
/* 200 */ 	

/* planner-control-your-spending.js */

/* 201 */ 	function onCustomGoalReady(e){
/* 202 */ 		var customGoal = $(this).val();
/* 203 */ 		trace('customGoal ' + customGoal);
/* 204 */ 		shebangSaveValue('cys_custom_goal', customGoal);
/* 205 */ 		shebangSaveValue('cys_goal_name', customGoal);
/* 206 */ 	}
/* 207 */ 	
/* 208 */ 	function onYearlyReady(){
/* 209 */ 		var perYear = $('#goal-annual-input').val();
/* 210 */ 		perYear = toNumber(perYear);
/* 211 */ 		var perMonth = Math.round(perYear / 12);
/* 212 */ 		calcMonthToOverall(perMonth);
/* 213 */ 		$('#monthly-savings-input').val(addCommas(perMonth));
/* 214 */ 		
/* 215 */ 		var pyf = addCommas(Math.round(perYear));
/* 216 */ 		shebangSaveValue('cys_yearly_goal', pyf);
/* 217 */ 	}
/* 218 */ 	
/* 219 */ 	function calcMonthToOverall(perMonth){
/* 220 */ 		if (isDateReady()){
/* 221 */ 			var delta = getMonthDelta();
/* 222 */ 			var overall = Math.round(perMonth * delta);
/* 223 */ 			var pmf = addCommas(Math.round(perMonth));
/* 224 */ 			if (overall){
/* 225 */ 				$('#overall-savings-input').val(addCommas(overall));
/* 226 */ 			}
/* 227 */ 			updateMonthlyFields(pmf);
/* 228 */ 		}
/* 229 */ 	}
/* 230 */ 	
/* 231 */ 	function calcSavingsNeededPerMonth(amt, months){
/* 232 */ 		var perMonth = 0;
/* 233 */ 		var ir = toNumber(shebangGetValue('cys-user-interest-rate')) / 100;
/* 234 */ 		trace('ir = ' + ir);
/* 235 */ 		if (ir){
/* 236 */ 			var c = ir/12;
/* 237 */ 			var n = months;
/* 238 */ 			var numerator = amt * c;
/* 239 */ 			var denominator = Math.pow(1 + c, n-1);
/* 240 */ 			//denominator = 1.38
/* 241 */ 			perMonth = numerator/denominator;
/* 242 */ 			//perMonth = 17.57
/* 243 */ 		} else {
/* 244 */ 			perMonth = amt / months;
/* 245 */ 		}
/* 246 */ 		return Math.round(perMonth);	
/* 247 */ 	}
/* 248 */ 	
/* 249 */ 	function calcOverallToMonth(amt){
/* 250 */ 		var delta = getMonthDelta();

/* planner-control-your-spending.js */

/* 251 */ 		var perMonth = calcSavingsNeededPerMonth(amt, delta);
/* 252 */ 		updateMonthlyFields(perMonth);
/* 253 */ 		$('#monthly-savings-input').val(perMonth);
/* 254 */ 		shebangSaveValue('cys_monthly_goal', perMonth);
/* 255 */ 		$('.todo-target .goal').text(perMonth);
/* 256 */ 	}
/* 257 */ 	
/* 258 */ 	function calcOverallToYear(amt){
/* 259 */ 		var delta = getMonthDelta();
/* 260 */ 		var perMonth = amt/delta;
/* 261 */ 		var perYear = Math.round(perMonth * 12);
/* 262 */ 		$('#goal-annual-input').val(perYear);
/* 263 */ 		shebangSaveValue('cys_yearly_goal', perYear);
/* 264 */ 	}
/* 265 */ 	
/* 266 */ 	function updateMonthlyFields(perMonth){
/* 267 */ 		$('.enter-goal-bar-saved .value.saved').text(perMonth);
/* 268 */ 		$('.enter-goal-summary .value.goal').text(addCommas(perMonth));
/* 269 */ 		$('.enter-goal-summary').fadeIn();
/* 270 */ 		$('#savings-menu .value.goal').text(perMonth);
/* 271 */ 	}
/* 272 */ 	
/* 273 */ 	function onMonthlyReady(){
/* 274 */ 		var perMonth = $('#monthly-savings-input').val();
/* 275 */ 		perMonth = toNumber(perMonth);
/* 276 */ 		calcMonthToOverall(perMonth);
/* 277 */ 		var perYear = Math.round(perMonth * 12);
/* 278 */ 		$('#goal-annual-input').val(Math.round(perYear)); 
/* 279 */ 		shebangSaveValue('cys_monthly_goal', perMonth);
/* 280 */ 	}
/* 281 */ 	
/* 282 */ 	function getMonthDelta(){
/* 283 */ 		var y = $('#finish-year-select').val();
/* 284 */ 		var m = $('#finish-month-select').val();
/* 285 */ 		var date = new Date();
/* 286 */ 		var dy = y - date.getFullYear();
/* 287 */ 		var dm = m - date.getMonth() + 1;
/* 288 */ 		var delta = dy * 12 + dm;
/* 289 */ 		return delta;
/* 290 */ 	}
/* 291 */ 	
/* 292 */ 	function isDateReady(){
/* 293 */ 		if (!$('#finish-year-select').length){
/* 294 */ 			return false;
/* 295 */ 		}
/* 296 */ 		var y = $('#finish-year-select').val().toLowerCase();
/* 297 */ 		var m = $('#finish-month-select').val().toLowerCase();
/* 298 */ 		if (y == 'default' || m == 'default'){
/* 299 */ 			return false;
/* 300 */ 		}

/* planner-control-your-spending.js */

/* 301 */ 		return true;
/* 302 */ 	}
/* 303 */ 	
/* 304 */ 	function onOverallReady(e){
/* 305 */ 		if (isDateReady()){
/* 306 */ 			var amt = $('#overall-savings-input').val();
/* 307 */ 			amt = toNumber(amt);
/* 308 */ 			calcOverallToMonth(amt);
/* 309 */ 			calcOverallToYear(amt);
/* 310 */ 		}
/* 311 */ 	}
/* 312 */ 	
/* 313 */ 	function initAddYourOwnTip(){
/* 314 */ 		$('.add-your-own-tip input:checkbox').each(function(){
/* 315 */ 			var id = 'ayo-'+Math.round(Math.random()*10000);
/* 316 */ 			var cb = $(this);
/* 317 */ 			var parent = $(this).closest('.pane').attr('id');
/* 318 */ 			cb.data('id', id);
/* 319 */ 			cb.data('parent', parent);
/* 320 */ 			cb.attr('data-parent', parent)
/* 321 */ 			cb.attr('data-id', id);
/* 322 */ 		})
/* 323 */ 		var af = $('.add-your-own-tip .spending-savings');
/* 324 */ 		$('.add-your-own-tip .add-your-own-name').blur(addYourOwnReady);
/* 325 */ 		af.bind('blur', addYourOwnReady);
/* 326 */ 		$('.add-your-own-tip').live('blur', addYourOwnReady);
/* 327 */ 	}
/* 328 */ 	
/* 329 */ 	function addYourOwnReady(){
/* 330 */ 		var tr = $(this).closest('.add-your-own-tip');
/* 331 */ 		var amtField = tr.find('.add-your-own-amount');
/* 332 */ 		var amt = amtField.val();
/* 333 */ 		var cb = tr.find('input:checkbox');
/* 334 */ 		var id = cb.data('id');
/* 335 */ 		var nameField = tr.find('.add-your-own-name');
/* 336 */ 		var text = nameField.val();
/* 337 */ 		var parent = cb.data('parent');
/* 338 */ 		if (!amt){
/* 339 */ 			trace('amount not ready');
/* 340 */ 			amtField.focus();
/* 341 */ 			return;
/* 342 */ 		}
/* 343 */ 		if (!text){
/* 344 */ 			trace('item not ready');
/* 345 */ 			nameField.focus();
/* 346 */ 			return;
/* 347 */ 		}
/* 348 */ 		if (parseInt(amt) == 0 || !amt){
/* 349 */ 			//no amount entered
/* 350 */ 			amtField.focus();

/* planner-control-your-spending.js */

/* 351 */ 		} else {
/* 352 */ 			cb.attr('checked', 'checked');
/* 353 */ 			save_todo(text, amt, id, parent);
/* 354 */ 			sendToActionPlan(cb, text, amt, id, true);
/* 355 */ 			updateSavings();
/* 356 */ 		}
/* 357 */ 	}
/* 358 */ 
/* 359 */ 	function makeSmallExpensesEditable(){
/* 360 */ 		$('td.spending-savings').each(function(){
/* 361 */ 			var amt = toNumber($(this).text());
/* 362 */ 			if ($(this).text() == '$-'){
/* 363 */ 				amt = '-';
/* 364 */ 			}
/* 365 */ 			if (amt){
/* 366 */ 				var input = $('<span class="dollar">$</span><input type="text" value="'+amt+'" class="spending-savings blank-input add-your-own-amount instant-blanking">');
/* 367 */ 			} else {
/* 368 */ 				var input = $('<span class="dollar">$</span><input type="text" placeholder="0" class="spending-savings blank-input add-your-own-amount">');
/* 369 */ 			}
/* 370 */ 			$(this).html(input);
/* 371 */ 		});
/* 372 */ 		$('.instant-blanking').focus(function(){
/* 373 */ 			var input = $(this);
/* 374 */ 			if (input.val() == input.attr('placeholder')){
/* 375 */ 				input.val('');
/* 376 */ 			}
/* 377 */ 		});
/* 378 */ 		$('.instant-blanking').blur(function(){
/* 379 */ 			var input = $(this);
/* 380 */ 			if (!input.val()){
/* 381 */ 				input.val(input.attr('placeholder'));
/* 382 */ 			}
/* 383 */ 		});
/* 384 */ 		
/* 385 */ 		$('.add-your-own-amount').live('blur', function(){
/* 386 */ 			var input = $(this);
/* 387 */ 			var cb = input.closest('tr').find('input:checkbox');
/* 388 */ 			cb.attr('data-amount', input.val());
/* 389 */ 			var id = cb.data('id');
/* 390 */ 			update_todo_field(id, 'amount', input.val());
/* 391 */ 			updateSavings();
/* 392 */ 			//getCheckedToDos();
/* 393 */ 		});
/* 394 */ 		$('.add-your-own-name').live('blur', function(){
/* 395 */ 			var input = $(this);
/* 396 */ 			var cb = input.closest('tr').find('input:checkbox');
/* 397 */ 			cb.attr('value', input.val());
/* 398 */ 			var id = cb.data('id');
/* 399 */ 			update_todo_field(id, 'value', input.val());
/* 400 */ 			//getCheckedToDos();

/* planner-control-your-spending.js */

/* 401 */ 		})
/* 402 */ 		
/* 403 */ 	}
