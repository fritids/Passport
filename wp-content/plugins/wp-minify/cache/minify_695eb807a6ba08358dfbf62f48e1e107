/* 331 */ function redrawBarGraphBar(htmlID, value, maxVal){
/* 332 */ 	var sel = '#graph-'+htmlID;
/* 333 */ 	var obj = $(sel);
/* 334 */ 	if (!obj.length){
/* 335 */ 		obj = $('#'+htmlID);
/* 336 */ 	}
/* 337 */ 	obj.data('value', toNumber(value));
/* 338 */ 	var th = value * getGraphUnit(maxVal);
/* 339 */ 	if (th < 18){
/* 340 */ 		obj.addClass('too-small-bar');
/* 341 */ 	} else {
/* 342 */ 		obj.removeClass('too-small-bar');
/* 343 */ 	}
/* 344 */ 	
/* 345 */ 	if (th > 0){
/* 346 */ 		if (!obj.is(':visible')){
/* 347 */ 			obj.height(0);
/* 348 */ 		}
/* 349 */ 		obj.show();
/* 350 */ 		obj.height(th);

/* shebang.js */

/* 351 */ 		var to = obj.data('timeout');
/* 352 */ 		clearTimeout(to);
/* 353 */ 	} else {
/* 354 */ 		hideBarGraphBar(htmlID);
/* 355 */ 	}
/* 356 */ 	obj.data('height', th);
/* 357 */ }
/* 358 */ 
/* 359 */ function getBarGraphValue(htmlID){
/* 360 */ 	var sel = '#graph-'+htmlID;
/* 361 */ 	var obj = $(sel);
/* 362 */ 	if (!obj.length){
/* 363 */ 		obj = $('#'+htmlID);
/* 364 */ 	}
/* 365 */ 	return obj.data('value');
/* 366 */ }
/* 367 */ 
/* 368 */ function updateBarGraphBar(htmlID, value){
/* 369 */ 	var sel = '#graph-'+htmlID;
/* 370 */ 	var obj = $(sel);
/* 371 */ 	if (!obj.length){
/* 372 */ 		obj = $('#'+htmlID);
/* 373 */ 	}
/* 374 */ 	obj.find('.amount').html(''+addCommas(roundTo(value, 10)));
/* 375 */ 	obj.data('value', toNumber(value));
/* 376 */ 	if (value > 0){
/* 377 */ 		obj.fadeIn();
/* 378 */ 	}
/* 379 */ 	var id = $(htmlID);
/* 380 */ 	if (id.length){
/* 381 */ 		id.val(value);
/* 382 */ 	} else {
/* 383 */ 		var hid = $('<input type="hidden" />');
/* 384 */ 		hid.attr('id', htmlID);
/* 385 */ 		hid.val(value);
/* 386 */ 		$('body').append(hid);
/* 387 */ 	}
/* 388 */ }
/* 389 */ 
/* 390 */ function hideBarGraphBar(htmlID){
/* 391 */ 	var sel = '#graph-'+htmlID;
/* 392 */ 	var obj = $(sel);
/* 393 */ 	if (!obj.length){
/* 394 */ 		obj = $('#'+htmlID);
/* 395 */ 	}
/* 396 */ 	if (obj.css('display') == 'none'){
/* 397 */ 		return;
/* 398 */ 	}
/* 399 */ 	obj.height(0);
/* 400 */ 	obj.attr('data-height', 0);

/* shebang.js */

/* 401 */ 	obj.data('value', 0);
/* 402 */ 	obj.fadeOut();
/* 403 */ }
