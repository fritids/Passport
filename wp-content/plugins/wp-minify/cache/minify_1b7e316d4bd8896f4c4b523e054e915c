/* number-utils.js */

/* 1  */ ;(function() { 
/* 2  */ 
/* 3  */   function sanitize(nStr) {
/* 4  */     return typeof nStr ==="number" ? nStr : nStr.indexOf(',') === -1 ? parseFloat(nStr) : parseFloat(nStr.split(',').join(""));
/* 5  */   }
/* 6  */ 
/* 7  */   function roundPlaces(n, places) {
/* 8  */     n = typeof n === "number" ? n : sanitize( n );
/* 9  */     return Math.round(n*(Math.pow(10,places))) / Math.pow(10,places);
/* 10 */   }
/* 11 */ 
/* 12 */   function format( number, places ) {
/* 13 */     places && ( number = roundPlaces( number, places) );
/* 14 */     number = ""+number;
/* 15 */     var x = number.split('.');
/* 16 */     var x1 = typeof p ==="number" && p === 0 ? Math.round(parseFloat(number))+'' : x[0];
/* 17 */     var x2 = x.length > 1 && ((typeof places ==="number" && places !== 0) || typeof places ==="undefined") ?  x[1] : '';
/* 18 */     x2 = places ? '.'+(pad(x2, places)) : (x2.length ? '.'+x2 : '');
/* 19 */     var rgx = /(\d+)(\d{3})/;
/* 20 */     while (rgx.test(x1)) {
/* 21 */       x1 = x1.replace(rgx, '$1' + ',' + '$2');
/* 22 */     }
/* 23 */     return x1 + x2;
/* 24 */   }
/* 25 */ 
/* 26 */   function pad( num, len, padChar, dir) {
/* 27 */ 
/* 28 */     var STR_PAD_LEFT = 1;
/* 29 */     var STR_PAD_RIGHT = 2;
/* 30 */     var STR_PAD_BOTH = 3;
/* 31 */     var str = "" + num;
/* 32 */ 
/* 33 */     len = len || 0;
/* 34 */     padChar = padChar || 0;
/* 35 */     dir = STR_PAD_RIGHT;
/* 36 */ 
/* 37 */     if (len + 1 >= str.length) {
/* 38 */ 
/* 39 */       switch (dir){
/* 40 */ 
/* 41 */       case STR_PAD_LEFT:
/* 42 */         str = Array(len + 1 - str.length).join(padChar) + str;
/* 43 */         break;
/* 44 */ 
/* 45 */       case STR_PAD_BOTH:
/* 46 */         var right = Math.ceil((padCharlen = len - str.length) / 2);
/* 47 */         var left = padCharlen - right;
/* 48 */         str = Array(left+1).join(padChar) + str + Array(right+1).join(padChar);
/* 49 */         break;
/* 50 */ 

/* number-utils.js */

/* 51 */       default:
/* 52 */         str = str + Array(len + 1 - str.length).join(padChar);
/* 53 */         break;
/* 54 */ 
/* 55 */       }
/* 56 */ 
/* 57 */     }
/* 58 */ 
/* 59 */     return str;
/* 60 */ 
/* 61 */   }
/* 62 */   window.numberUtils = {
/* 63 */     format: format,
/* 64 */     formatRate: function( rate ) {
/* 65 */       return format( rate, 3 );
/* 66 */     },
/* 67 */     pad: pad
/* 68 */   };
/* 69 */ 
/* 70 */ })();
/* 71 */ 

;
/* jquery.fixed-scroll.js */

/* 1  */ ;(function($){
/* 2  */ 	$.fn.fixedScroll = function(opts){
/* 3  */ 	
/* 4  */ 		var defaults = {fixedClass:'fixed', absClass:'relative', container:'parent', offset:0, forceTop:false, topLimit:null};
/* 5  */ 		var opts = $.extend(defaults, opts);
/* 6  */ 		
/* 7  */ 		return this.each(function(){
/* 8  */ 			var item = $(this);
/* 9  */ 			var over;
/* 10 */ 			var width = $(this).width();
/* 11 */ 			if (width != $(this).outerWidth()){
/* 12 */ 				width = $(this).outerWidth();
/* 13 */ 			}
/* 14 */ 			var container = item.parent();
/* 15 */ 			if (opts.container != 'parent'){
/* 16 */ 				container = opts.container;
/* 17 */ 				if (typeof container == 'string'){
/* 18 */ 					container = $(container);
/* 19 */ 				}
/* 20 */ 			}
/* 21 */ 			var topLimit = item.offset().top - container.offset().top + opts.offset;
/* 22 */ 			if (opts.topLimit != null){
/* 23 */ 				topLimit = opts.topLimit;
/* 24 */ 			}
/* 25 */ 			$(window).bind('scroll resize', onScrollCheck);
/* 26 */ 			container.bind('resize', onScrollCheck);
/* 27 */ 			
/* 28 */ 			function onScrollCheck(){
/* 29 */ 				var wt = $(window).scrollTop();
/* 30 */ 				var gt = 0;
/* 31 */ 				if (item.data('oy')){
/* 32 */ 					gt = item.data('oy');
/* 33 */ 				} else {
/* 34 */ 					gt = item.offset().top;
/* 35 */ 					item.data('oy', gt);
/* 36 */ 				}	
/* 37 */ 				var dist = gt - wt;
/* 38 */ 				if (dist < topLimit){
/* 39 */ 					//going fixed
/* 40 */ 					item.addClass(opts.fixedClass);
/* 41 */ 					item.removeClass(opts.absClass);
/* 42 */ 					item.width(width);
/* 43 */ 				} else {
/* 44 */ 					//going relative
/* 45 */ 					item.removeClass(opts.fixedClass);
/* 46 */ 					item.removeClass(opts.absClass);
/* 47 */ 				}
/* 48 */ 				if (wt > container.offset().top + container.height() - item.outerHeight()){
/* 49 */ 					item.removeClass(opts.fixedClass);
/* 50 */ 					item.addClass(opts.absClass);

/* jquery.fixed-scroll.js */

/* 51 */ 					var top = container.height() - item.outerHeight();
/* 52 */ 					item.css('top', top);
/* 53 */ 				} else {
/* 54 */ 					if (opts.forceTop){
/* 55 */ 						item.css('top', '0px');
/* 56 */ 					} else {
/* 57 */ 						item.css('top', '');
/* 58 */ 					}
/* 59 */ 				}
/* 60 */ 			}
/* 61 */ 		});
/* 62 */ 	}
/* 63 */ })(jQuery);

;
