/* 154 */ 			if (!children){
/* 155 */ 				goodbye.remove();
/* 156 */ 				var td = tr.find('td').first();
/* 157 */ 				td.attr('colspan', 10);
/* 158 */ 				td.wrapInner('<div class="content"></div>');
/* 159 */ 			} else {
/* 160 */ 			}
/* 161 */ 		}
/* 162 */ 		
/* 163 */ 		if (rowClass == 'gap'){
/* 164 */ 			
/* 165 */ 		}
