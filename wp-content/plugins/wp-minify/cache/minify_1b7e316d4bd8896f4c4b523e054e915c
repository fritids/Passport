/* 1  */ ;(function($){
/* 2  */ 	$.fn.sailTo = function(dest, opts){
/* 3  */ 	
/* 4  */ 		var defaults = {duration:800, text:false, textTarget:null};
/* 5  */ 		
/* 6  */ 		var options = $.extend(defaults, opts);
/* 7  */ 	
/* 8  */ 		var sailor;
/* 9  */ 	
/* 10 */ 		return this.each(function(){
/* 11 */ 			var starter = $(this);
/* 12 */ 			sailor = $('<div class="sailor" />');
/* 13 */ 			var props = {'width':starter.width(), height:starter.height(), position:'absolute', top:starter.offset().top, left:starter.offset().left};
/* 14 */ 			sailor.css(props);
/* 15 */ 			$('#container').append(sailor);
/* 16 */ 			var goTo = $(dest);
/* 17 */ 			sailor.animate({width:goTo.width(), height:goTo.height(), top:goTo.offset().top, left:goTo.offset().left, opacity:0}, options.duration, function(){
/* 18 */ 				$(this).remove();
/* 19 */ 			});
/* 20 */ 			
/* 21 */ 			if (options.text){
/* 22 */ 				var ns = $('<div class="sailor-text" />');
/* 23 */ 				ns.css(props);
/* 24 */ 				ns.css('z-index', 55000);
/* 25 */ 				ns.text(options.text);
/* 26 */ 				ns.css('opacity', 0);
/* 27 */ 				$('#container').append(ns);
/* 28 */ 				if ($(options.textTarget).length){
/* 29 */ 					goTo = $(options.textTarget);
/* 30 */ 					var fontFamily = goTo.css('font-family');
/* 31 */ 					var fontSize = goTo.css('font-size');
/* 32 */ 					var color = goTo.css('color');
/* 33 */ 					ns.css('font-family', fontFamily);
/* 34 */ 					ns.css('font-size', fontSize);
/* 35 */ 					ns.css('color', color);
/* 36 */ 					goTo.wrapInner('<span class="fader" />');
/* 37 */ 					$('.fader').animate({opacity:0});
/* 38 */ 				}
/* 39 */ 				
/* 40 */ 				var top = goTo.offset().top;
/* 41 */ 				var left = goTo.offset().left;
/* 42 */ 				trace(goTo);
/* 43 */ 				trace(options);
/* 44 */ 				ns.animate({top:top, left:left, opacity:1}, options.duration, function(){
/* 45 */ 					var nt = $(this);
/* 46 */ 					nt.delay(1500).fadeOut(600, function(){
/* 47 */ 						$(this).remove();
/* 48 */ 					});
/* 49 */ 				});
/* 50 */ 				

/* jquery.sailTo.js */

/* 51 */ 			}	
/* 52 */ 		});
/* 53 */ 	}
/* 54 */ })(jQuery);

