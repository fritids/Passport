/* 108 */ 		var test = '[class*="input"]';
/* 109 */ 		table.find('tr').each(function(){
/* 110 */ 			var tr = $(this);
/* 111 */ 			if (tr.filter(test).length){
/* 112 */ 				if (!tr.prev(test).length){
