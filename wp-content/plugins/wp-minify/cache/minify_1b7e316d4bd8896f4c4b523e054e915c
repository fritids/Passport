/* 213 */ 		if (phtml){
/* 214 */ 			if (!p.html().length){
/* 215 */ 				
/* 216 */ 			}
/* 217 */ 			if (!phtml.trim().length){
/* 218 */ 				p.remove();
/* 219 */ 			}
/* 220 */ 			if(p.html().trim() == ''){
/* 221 */ 				p.remove();
/* 222 */ 			}
/* 223 */ 		}
/* 224 */ 	});
/* 225 */ 	
/* 226 */ 	/* Wrap <p>s in lists */
/* 227 */ 	$('.ui-article-block li').wrapInner('<p>');
/* 228 */ }
