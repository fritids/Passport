/* 45  */ 
/* 46  */   RefiAnalyzer.prototype.setupInputs = function() {
/* 47  */     var $inputs = this.$form.find("input, select"),
/* 48  */         $yearsIndicators = $("span.mtg-length-yrs");
/* 49  */ 	
/* 50  */     // Run through each of the parameters, fetching the input with that parameter name and

/* calculators-should-you-refinance.js */

/* 51  */     // setting it to the default or saved value for the parameter, and setting up the calculation when the inputs change
/* 52  */     $.each( this.parameters, $.proxy( function(name, defaultValue) {
/* 53  */ 
/* 54  */       // Get the initial value out of localStorage or use the default;
/* 55  */       var value = ls.getItem( name ) || defaultValue;
/* 56  */ 
/* 57  */       // Store the value on this instance, in case it was fetched from localStorage
/* 58  */       this.parameters[ name ] = value;
/* 59  */       // Store a reference to the input element inside of the elements cache
/* 60  */       this.elements[ "$" + name ] = $inputs.filter("[name='"+ name +"']")
/* 61  */       .bind("keypress change blur click", $.proxy(function(e) {
/* 62  */         // Get the current element off the elements cache instead of creating a redundant jQuery
/* 63  */         var $el = this.elements[ "$" + name ],
/* 64  */             v = $el.val();
/* 65  */ 
/* 66  */         if ( $el.is("[type='checkbox']") ) { 
/* 67  */           // For the checkbox(es), we want to use the checkbox's checked state, not its actual value property
/* 68  */           v = $el.is(":checked");
/* 69  */         } else if ( $el.is("[type='number']") ) {
/* 70  */           // Handle numeric inputs
/* 71  */           // If an element has a max and it is presently above it, clamp it to the max
/* 72  */           // Uses HTML5 validity API
/* 73  */           if ( e.target.validity.rangeOverflow ) {
/* 74  */             v = $el.attr("max");
/* 75  */             $el.val( v );
/* 76  */           }
/* 77  */ 
/* 78  */           // If an element has a minimum and it is presently below it (negative numbers),
/* 79  */           // reset to last good stored value
/* 80  */           // Uses HTML5 validity API
/* 81  */           if ( e.target.validity.rangeUnderflow ) {
/* 82  */             v = this.parameters[ name ];
/* 83  */             $el.val( v );
/* 84  */           }
/* 85  */         }
/* 86  */ 
/* 87  */         // Set the parameter on the RefiAnalyzer from the value
/* 88  */         // and save the value in localStorage
/* 89  */         this.parameters[ name ] = v;
/* 90  */         ls.setItem( name, v );
/* 91  */ 
/* 92  */       }, this))
/* 93  */       // Set the value of the input to the current (possibly restored) value
/* 94  */       .val( value )
/* 95  */       // Recalculate the analysis 100ms after any amount of input has concluded
/* 96  */       // Allows for tpying to conplete, etc, to cut down on *really* redundant calculations
/* 97  */       .bind("keypress change blur click", $.proxy(function(e) {
/* 98  */         this.calcDelay && clearTimeout( this.calcDelay );
/* 99  */         this.calcDelay = setTimeout( $.proxy( this.calculate, this ), 100 );
/* 100 */       }, this));

/* calculators-should-you-refinance.js */

/* 101 */ 
/* 102 */     }, this));
/* 103 */ 
/* 104 */     // Set the current value for the get_cash_out_impact checkbox and the parameter from localStorage, casting it from string to boolean
/* 105 */     this.elements.$get_cash_out_impact[0].checked = this.parameters.get_cash_out_impact = this.parameters.get_cash_out_impact == "true";
/* 106 */ 
/* 107 */     // Set the value of the holding period in years from the parameter
/* 108 */     // in case it was updated via localStorage
/* 109 */     $yearsIndicators = $("span.mtg-length-yrs").text( this.parameters.mortgage_holding_period_years );
/* 110 */     this.elements.$mortgage_holding_period_years
/* 111 */     .val ( this.parameters.mortgage_holding_period_years )
/* 112 */     // When the number of years changes, set the relevant displays of that value
/* 113 */     .change(function(e) {
/* 114 */       $yearsIndicators.text( e.target.value );
/* 115 */     });
/* 116 */ 
/* 117 */     this.$form.delegate("select.mtg-compare-select", "change", $.proxy(function(e) {
/* 118 */ 
/* 119 */       // Store the choice in RefiAnalyzer paramters and localStorage
/* 120 */       var field = e.target.name;
/* 121 */       this.parameters[ field ] = e.target.value;
/* 122 */       ls.setItem( field, e.target.value );
/* 123 */       ls.removeItem( "refi_interest_rate" );
/* 124 */       // Set the mortgages (uses the stored value);
/* 125 */       this.setMortgages();
/* 126 */     },this));
/* 127 */ 
/* 128 */   };
/* 129 */ 
/* 130 */   RefiAnalyzer.prototype.calcDelay = false;
/* 131 */ 
/* 132 */   RefiAnalyzer.prototype.calculate = function() {
/* 133 */     var refi_type = this.parameters.refi_mortgage_type,
/* 134 */         ARM = false,
/* 135 */         outputs = $.map( this.mortgages, function( mortgage, type ) {
/* 136 */           ARM = mortgage.ARM;
/* 137 */           return mortgage.calculate();
/* 138 */         }),
/* 139 */         bt_impact = outputs[0].monthly_mortgage_payment_bt - outputs[1].monthly_mortgage_payment_bt,
/* 140 */         at_impact = outputs[0].monthly_mortgage_payment_at - outputs[1].monthly_mortgage_payment_at,
/* 141 */         same_dollar_impact = Math.abs( at_impact ) < 25,
/* 142 */         owed_diff = outputs[0].loan_remaining_current_dollars - outputs[1].loan_remaining_current_dollars,
/* 143 */         owed_diff_pct = Math.abs( outputs[1].loan_remaining_current_dollars / outputs[0].loan_remaining_current_dollars ),
/* 144 */         same_owed_pct = owed_diff_pct >= 0.95 && owed_diff_pct <= 1.05,
/* 145 */ 
/* 146 */         monthlyLessOrMore = bt_impact > 0 ? "less" : "more",
/* 147 */         owedLessOrMore = owed_diff > 0 ? "less" : "more",
/* 148 */ 
/* 149 */         closing_cost_recoup_years = Math.round( outputs[1].closing_costs / bt_impact / 12 ),
/* 150 */         closing_cost_recoup_years = closing_cost_recoup_years < 1 ? 1 : closing_cost_recoup_years,

/* calculators-should-you-refinance.js */

/* 151 */ 
/* 152 */         // Only show the closing cost savings payoff if
/* 153 */         // 1) The monthly payment is actually less in the refi
/* 154 */         // 2) The interest rate on the refi loan is lower than the interest rate on the current loan
/* 155 */         // 3) The refi loan will be paid off within 5 years of the time the original loan would have been paid off
/* 156 */         show_closing_cost_calcs = bt_impact >= 0 &&
/* 157 */                                   (this.mortgages.refi.interest_rate <= this.mortgages.current.interest_rate) &&
/* 158 */                                   (this.mortgages.refi.length - this.parameters.mortgage_yrs_left <= 5),
/* 159 */ 
/* 160 */         cash_out_exists = outputs[1].cash_out_max > 0;
/* 161 */ 
/* 162 */     this.$explainer.find(".payment-diff-bt").text("$" + numberUtils.format(Math.abs(bt_impact)) ).toggleClass("more", monthlyLessOrMore == "more");
/* 163 */     this.$explainer.find(".payment-diff-at").text("$" + numberUtils.format(Math.abs(at_impact)) ).toggleClass("more", monthlyLessOrMore == "more");
/* 164 */     this.$explainer.find(".mo-payment-gt-lt").text( monthlyLessOrMore ).toggleClass("more", monthlyLessOrMore == "more");
/* 165 */     this.$explainer.find(".arm-only").toggle( this.mortgages.refi.ARM );
/* 166 */ 
/* 167 */     this.$explainer.find(".closing-costs").text( "$" + numberUtils.format(outputs[1].closing_costs) );
/* 168 */     this.$explainer.find(".exp-yrs-to-cover").text( closing_cost_recoup_years );
/* 169 */     this.$explainer.find(".yrs-to-cover-plural").toggle( closing_cost_recoup_years > 1 );
/* 170 */     this.$explainer.find(".cc-calcs").toggle( show_closing_cost_calcs );
/* 171 */ 
/* 172 */     this.$explainer.find(".same-pct-show").toggle( same_owed_pct ).next(".same-pct-hide").toggle( !same_owed_pct );
/* 173 */     this.$explainer.find(".same-dollar-show").toggle( same_dollar_impact ).next(".same-dollar-hide").toggle( !same_dollar_impact );
/* 174 */ 
/* 175 */     this.$explainer.find(".amount-owed-gt-lt").text( owedLessOrMore ).toggleClass("more", owedLessOrMore == "more");
/* 176 */     this.$explainer.find(".amt-owed-diff").text("$" + numberUtils.format(Math.abs( owed_diff )) ).toggleClass("more", owedLessOrMore == "more");
/* 177 */     this.$explainer.find(".amt-owed-refi").text("$" + numberUtils.format(outputs[1].loan_remaining_current_dollars) );
/* 178 */     this.$explainer.find(".amt-owed-current").text("$" + numberUtils.format(outputs[0].loan_remaining_current_dollars) );
/* 179 */ 
/* 180 */     this.$explainer.find(".lower-payment-data .is-isnt-switch").text( at_impact < 0 || same_dollar_impact ? "isn't" : "is" );
/* 181 */     this.$explainer.find(".pay-faster-data .is-isnt-switch").text( owed_diff < 0 || same_owed_pct ? "isn't" : "is" );
/* 182 */     this.$explainer.find(".home-value").text( "$" + numberUtils.format(this.parameters.home_value) );
/* 183 */     this.$explainer.find(".cash-out-limit").text( "$" + numberUtils.format(this.parameters.home_value * 0.75) );
/* 184 */     this.$explainer.find(".cash-out-max").text( "$" + numberUtils.format( outputs[1].cash_out_max) );
/* 185 */     this.$explainer.find(".cash-out-exists-show").toggle( cash_out_exists ).next().toggle( !cash_out_exists );
/* 186 */ 
/* 187 */   };
/* 188 */ 
/* 189 */   RefiAnalyzer.prototype.setMortgages = function( types ) {
/* 190 */     this.$tbody.empty();
/* 191 */     this.mortgages = {};
/* 192 */     this.addMortgage( this.parameters.current_mortgage_type, "current");
/* 193 */     this.addMortgage( this.parameters.refi_mortgage_type, "refi" );
/* 194 */     this.calculate();
/* 195 */   };
/* 196 */ 
/* 197 */   RefiAnalyzer.prototype.addMortgage = function( mortgageType, role ) {
/* 198 */     this.mortgages[ role ] = new Mortgage( mortgageType, role, this );
/* 199 */ 
/* 200 */     //If we're adding an ARM, we have to add the 'adjusted' version as well

/* calculators-should-you-refinance.js */

/* 201 */     if ( role == "refi" && this.mortgages[ role ].ARM ) {
/* 202 */       this.mortgages[ role + "_adjusted" ] = new Mortgage( mortgageType + "_adjusted", role, this );
/* 203 */     }
/* 204 */   };
/* 205 */ 
/* 206 */   function Mortgage( type, role, analyzer) {
/* 207 */     this.role = role;
/* 208 */     this.analyzer = analyzer;
/* 209 */     this.ARM = !!( ~type.indexOf("arm") );
/* 210 */     this.adjusted = !!( ~type.indexOf("adjusted") );
/* 211 */     type = this.analyzer.mortgage_types[ type ];
/* 212 */     $.extend( this, type );
/* 213 */     if ( this.role == "current" && this.ARM ) {
/* 214 */       this.interest_rate = analyzer.mortgage_types[ type.mort_type + "_adjusted"].interest_rate;
/* 215 */     }
/* 216 */     if ( this.adjusted ) {
/* 217 */       var ua = ls.getItem("use_adjustment");
/* 218 */       this.use_adjustment = ua && ua == "true" ? true : false;
/* 219 */       this.interest_rate_jump = parseFloat( analyzer.mortgages.refi.interest_rate ) + 0.06;
/* 220 */     } else {
/* 221 */       this.interest_rate = ls.getItem(this.role+"_interest_rate") || this.interest_rate;
/* 222 */     }
/* 223 */     this.$row = $(Mortgage.template( this, analyzer )).appendTo( analyzer.$tbody ).data( "mortgage", this );
/* 224 */     this.calcDelay = false;
/* 225 */     this.$row.delegate("input.mtg-int-input", "change click", $.proxy(function(e) {
/* 226 */       this.calcDelay && clearTimeout(this.calcDelay);
/* 227 */       this.calcDelay = setTimeout($.proxy(function(){
/* 228 */         this.interest_rate = e.target.value / 100;
/* 229 */         if ( this.role == "refi" && this.ARM && analyzer.mortgages.refi_adjusted.use_adjustment ) {
/* 230 */           var r = analyzer.mortgages.refi_adjusted.interest_rate_jump = this.interest_rate + 0.06;
/* 231 */           analyzer.mortgages.refi_adjusted.$row.find(".mtg-int-adjusted").text( Math.round(r * 10000) / 100 );
/* 232 */         }
/* 233 */         ls.setItem(this.role+"_interest_rate", this.interest_rate );
/* 234 */         analyzer.calculate();
/* 235 */       },this),100);
/* 236 */     },this));
/* 237 */ 
/* 238 */     if ( this.adjusted ) {
/* 239 */       this.$row.delegate("input[name='use_adjustment']", "change", $.proxy(function(e) {
/* 240 */         this.use_adjustment = $(e.target).is(":checked");
/* 241 */         ls.setItem("use_adjustment", this.use_adjustment);
/* 242 */         this.$row.find(".mtg-int-adjusted").text( Math.round(this[ "interest_rate" +( this.use_adjustment ? "_jump" : "" ) ] * 10000) / 100);
/* 243 */         analyzer.calculate();
/* 244 */       },this));
/* 245 */     }
/* 246 */     this.parameters = $.extend({}, this.parameters);
/* 247 */   }
/* 248 */ 
/* 249 */   Mortgage.prototype.parameters = {
/* 250 */     home_equity: 0,

/* calculators-should-you-refinance.js */

/* 251 */     loan_remaining_current_dollars: 0,
/* 252 */     monthly_mortgage_payment_bt: 0,
/* 253 */     monthly_mortgage_payment_at: 0,
/* 254 */     closing_costs: 0
/* 255 */   };
/* 256 */ 
/* 257 */   Mortgage.prototype.calculate = function() {
/* 258 */ 
/* 259 */     // Step 0
/* 260 */     // Calculate intermediate values, including the original loan amount
/* 261 */ 
/* 262 */     var parms = this.analyzer.parameters,
/* 263 */ 
/* 264 */         // Get the right interest rate to use
/* 265 */         rate = this.ARM && this.adjusted && this.use_adjustment ? this.interest_rate_jump : this.interest_rate,
/* 266 */ 
/* 267 */         // Percentage points of interest to buy down (select the appropriate version depending if it's an ARM)
/* 268 */         points = this.role == "current" ? 0 : parseInt( parms[ "mortgage_discount_points_" + (this.ARM ? "arm" : "fixed") ], 10) / 100,
/* 269 */ 
/* 270 */         // Length of mortgage in months
/* 271 */         length_months = this.length * 12,
/* 272 */ 
/* 273 */         // Effective interest rate after points are factored in
/* 274 */         interest_rate_adj = parseFloat( rate ) - (0.125 * points ),
/* 275 */ 
/* 276 */         // Monthly interest rate
/* 277 */         monthly_interest_rate = interest_rate_adj / 12,
/* 278 */ 
/* 279 */         // Loan amount - calculate the original loan amount for the current loan, grab the refinance amount from the current loan's calculation for the refi.
/* 280 */         amount = parseInt(this.analyzer.parameters.mortgage_amount_remaining, 10),
/* 281 */ 
/* 282 */         // Number of months the mortgage will actually be held
/* 283 */         // Can't be longer than the actual length of the mortgage
/* 284 */         holding_period_months = parms.mortgage_holding_period_years * 12,
/* 285 */         holding_period_months = holding_period_months > length_months ? length_months : holding_period_months,
/* 286 */         // Total Closing Costs
/* 287 */         closing_costs = this.parameters.closing_costs = this.role == "current" ? 0 : parseInt(parms.closing_cost_fixed_amount,10),
/* 288 */ 
/* 289 */         // Figure out the maximum amount that could be used in a 'cash out' refinance
/* 290 */         // Can't be less than 0
/* 291 */         cash_out_max = this.role == "refi" ? (Math.round(this.analyzer.parameters.home_value * 0.75) - closing_costs - this.analyzer.parameters.mortgage_amount_remaining) : 0,
/* 292 */         cash_out_max = this.parameters.cash_out_max = cash_out_max < 0 ? 0 : cash_out_max,
/* 293 */ 
/* 294 */         // Adjusted amount of mortgage after points and closing costs are factored in
/* 295 */         // We only want to account for closing costs on the refinanced mortgage, not the current one
/* 296 */         amount_adj = amount + closing_costs + points + (this.role == "refi" && this.analyzer.parameters.get_cash_out_impact ? cash_out_max : 0);
/* 297 */ 
/* 298 */     // Step 1
/* 299 */     // Calculate the before-tax monthly payment (monthly_mortgage_payment_bt)
/* 300 */     // monthly_mortgage_payment_bt = mortgage_amount_adj * (monthly_interest / ( 1- (1+monthly_interest) ^ (-mortgage_length_months ) )  )

/* calculators-should-you-refinance.js */

/* 301 */     this.parameters.monthly_mortgage_payment_bt = this.role == "current" ? parseInt( this.analyzer.parameters.current_mortgage_payment, 10 ) : Math.round(( amount_adj * ( monthly_interest_rate / ( 1 - Math.pow( 1 + monthly_interest_rate, (-length_months) ) ) ) / 10) ) * 10;
/* 302 */ 
/* 303 */     // Step 2
/* 304 */     // Amortization - calculate and collect the impact of each monthly payment
/* 305 */     var i = 0,
/* 306 */         payments = [],
/* 307 */         itemize = !!parseInt( parms.itemize_flag, 10),
/* 308 */         marginal_tax_rate = parseInt( parms.marginal_tax_rate, 10 ) / 100,
/* 309 */         principal_remaining = amount_adj;
/* 310 */ 
/* 311 */ 
/* 312 */     function monthImpact( month ) {
/* 313 */       // Calculate each month in the loan term
/* 314 */       // Starting principal is the ending principal of the previous period or the original loan amount on the first iteration
/* 315 */       var start_principal = payments[ month-1 ] ? payments[ month-1 ].end_principal : amount_adj,
/* 316 */           interest_paid = start_principal * monthly_interest_rate,
/* 317 */           principal_paid = this.parameters.monthly_mortgage_payment_bt - interest_paid,
/* 318 */           end_principal = start_principal - principal_paid;
/* 319 */ 
/* 320 */       principal_remaining = end_principal;
/* 321 */ 
/* 322 */       return {
/* 323 */         start_principal: start_principal,
/* 324 */         principal_paid: principal_paid,
/* 325 */         interest_paid: interest_paid,
/* 326 */         tax_savings: itemize ? interest_paid * marginal_tax_rate : 0,
/* 327 */         end_principal: end_principal
/* 328 */       };
/* 329 */     }
/* 330 */ 
/* 331 */     while ( principal_remaining > 10 ) {
/* 332 */       payments.push( monthImpact.call( this, i ) );
/* 333 */       // Don't allow there to be more pay periods than there actually would be in the term of the mortgage
/* 334 */       if ( ++i > length_months ) {
/* 335 */         break;
/* 336 */       }
/* 337 */     }
/* 338 */ 
/* 339 */     // Step 3
/* 340 */     // Calculate remaining output variables
/* 341 */     var inflation_rate = parseFloat(this.analyzer.constants.inflation_rate) / 100,
/* 342 */         // Get the principal remaining for the last month the mortgage is held
/* 343 */         last_month = payments[ holding_period_months - 1 ] || payments.slice( -1 )[0];
/* 344 */ 
/* 345 */     // Inflation-adjust the remaining loan amount, round to the nearest thousand
/* 346 */     this.parameters.loan_remaining_current_dollars = Math.round( (last_month.end_principal / Math.pow( 1 + inflation_rate, parms.mortgage_holding_period_years )) / 1000 ) * 1000;
/* 347 */     // Logically cannot be less than zero
/* 348 */     this.parameters.loan_remaining_current_dollars = this.parameters.loan_remaining_current_dollars < 0 ? 0 : this.parameters.loan_remaining_current_dollars;
/* 349 */ 
/* 350 */     // Subtract the remaining amount from the home's value to get the percentage owned

/* calculators-should-you-refinance.js */

/* 351 */     this.parameters.home_equity = parseInt( parms.home_value, 10) - this.parameters.loan_remaining_current_dollars;
/* 352 */ 
/* 353 */     // Determine the after-tax payment using the tax savings from the first month of the amortization
/* 354 */     this.parameters.monthly_mortgage_payment_at = Math.round( (this.parameters.monthly_mortgage_payment_bt - payments[0].tax_savings) / 10 ) * 10;
/* 355 */ 
/* 356 */     // Display the calculated display parameters
/* 357 */     this.$row.find(".mtg-payment-data-"+this.role).text( numberUtils.format(this.parameters.monthly_mortgage_payment_bt) );
/* 358 */     this.$row.find(".mtg-payment-data-at-"+this.role).text( numberUtils.format(this.parameters.monthly_mortgage_payment_at) );
/* 359 */ 
/* 360 */     // Set the proper content of the interest-rate tooltip in the adjusted ARM
/* 361 */     if ( this.adjusted ) {
/* 362 */       this.$row.find(".initial-rate-show").toggle( !this.use_adjustment ).next().toggle( this.use_adjustment );
/* 363 */     }
/* 364 */ 
/* 365 */     return this.parameters;
/* 366 */   };
/* 367 */ 
/* 368 */   Mortgage.template = function( type, analyzer ) {
/* 369 */     return [ "<tr class='mtg-group-", type.role, " mtg-group-",type.mort_type, (type.adjusted ? " refi-mtg-group-adjusted ma-adjustable-after-group" : "") ,"'>",
/* 370 */       "<td class='mtg-compare-data'>",
/* 371 */         type.adjusted ? 
/* 372 */         "<span>" + type.pretty_name + "</span>" :
/* 373 */         ((type.role == 'current' ? 'Current' : 'Refinanced')+' Mortgage'), '</td>',
/* 374 */         ( type.adjusted ? ["<td class='rates-jump last'>",
/* 375 */           "<input id='rates-jump' class='rates-jump-checkbox checkbox-input' type='checkbox' name='use_adjustment' ",
/* 376 */           (type.use_adjustment ? "checked='checked'" : ""),
/* 377 */           "value='rates-jump' ","><label class='rates-jump-label checkbox-label' for='rates-jump'>See worst-case scenario</label>",
/* 378 */           "</td>"] :
/* 379 */           [ "<td class='mtg-type-data form-field-med'>",
/* 380 */             "<select name='",type.role,"_mortgage_type' id='mtg-compare-select' class='mtg-compare-select'>",
/* 381 */           (function() {
/* 382 */             return $.map( analyzer.mortgage_types, function( t ) {
/* 383 */               return ~t.mort_type.indexOf("adjusted") ? "" : "<option value='" + t.mort_type +"'"+ (t.mort_type == type.mort_type ? "selected" : "") +">"+ t.pretty_name +"</option>";
/* 384 */             }).join("");
/* 385 */           })(),
/* 386 */           "</select></td>"])
/* 387 */         .join(""),
/* 388 */         "</td>",
/* 389 */         "<td class='mtg-int-data ", (type.adjusted ? "mtg-int-data-adjusted" : "form-field-med-pct"), "'>",
/* 390 */         ( type.adjusted ?
/* 391 */           ["<span class='info-box tip-trigger'>",
/* 392 */             "<span class='mtg-int-adjusted'>",Math.round(type[ "interest_rate" +( type.use_adjustment ? "_jump" : "" ) ] * 10000) / 100,"</span>",
/* 393 */             "<span class='pct'>%</span>",
/* 394 */             "<div class='tooltip'>",
/* 395 */               "<span class='initial-rate-show'>",
/* 396 */                 "This rate is based on an estimated "+((type.interest_rate * 100) - 3).toFixed(2)+"% index rate plus a 3% margin.",
/* 397 */               "</span>",
/* 398 */               "<span class='initial-rate-hide'>",
/* 399 */                 "This is your initial interest rate plus 6%, which is the cap for most adjustable-rate mortgages.",
/* 400 */               "</span>",

/* calculators-should-you-refinance.js */

/* 401 */             "</div>",
/* 402 */             "</span>"
/* 403 */           ].join("") :
/* 404 */           "<input class='mtg-int-input mtg-int-current' name='"+type.role+"_interest_rate' value='"+ type.interest_rate * 100+"' type='number' placeholder='4.0' step='0.125'><span class='pct'>%</span>"
/* 405 */         ),
/* 406 */         "</td>",
/* 407 */         "<td class='mtg-payment-data'>",
/* 408 */           "<span class='dollar'>$</span>",
/* 409 */           "<span class='mtg-payment-data-",type.role ,"'>0</span>",
/* 410 */         "</td>",
/* 411 */         "<td class='mtg-payment-data-at'>",
/* 412 */           "<span class='dollar'>$</span><span class='mtg-payment-data-at-", type.role ,"'>134</span>",
/* 413 */         "</td>",
/* 414 */       "</tr>"
/* 415 */     ].join("");
/* 416 */   };
/* 417 */ 
/* 418 */   $(function() {
/* 419 */ 
/* 420 */     var analyzer = new RefiAnalyzer({
/* 421 */       $form: $("form.refi-form"),
/* 422 */       $tbody: $("#refi-compare-table tbody"),
/* 423 */       $explainer: $("section.compare-explainer")
/* 424 */   });
/* 425 */ 
/* 426 */   });
/* 427 */ 
/* 428 */ })();
