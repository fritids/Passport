/* 302 */ function getGraphMaxVal(){
/* 303 */ 	var max = 0;
/* 304 */ 	$('.bar-graph-bar').each(function(){
/* 305 */ 		var val = toNumber($(this).data('value'));
/* 306 */ 		if (val > 0){
/* 307 */ 			max += val; 
/* 308 */ 		}
/* 309 */ 	});
/* 310 */ 	return max;
/* 311 */ }
/* 312 */ 
/* 313 */ function redrawBarGraph(maxVal){
/* 314 */ 	if (!maxVal){
/* 315 */ 		maxVal = getGraphMaxVal();
/* 316 */ 	}
/* 317 */ 	$('.bar-graph-bar').each(function(){
/* 318 */ 		var htmlID = $(this).attr('id');
/* 319 */ 		var val = $(this).find('.amount').html();
/* 320 */ 		if (val == '' || val == null){
/* 321 */ 			return;
/* 322 */ 		}
/* 323 */ 		if (val){
/* 324 */ 			val = toNumber(val.replace('$', ''));
/* 325 */ 		}
/* 326 */ 		if (val){
/* 327 */ 			updateBarGraphBar(htmlID, val);
/* 328 */ 		} else {
/* 329 */ 			hideBarGraphBar(htmlID);
/* 330 */ 		}
/* 331 */ 	});
/* 332 */ 	$('.bar-graph-bar').each(function(){
/* 333 */ 		var htmlID = $(this).attr('id');
/* 334 */ 		var val = $(this).data('value');
/* 335 */ 		if (val){
/* 336 */ 			redrawBarGraphBar(htmlID, val, maxVal);
/* 337 */ 		} else {
/* 338 */ 			hideBarGraphBar(htmlID);
/* 339 */ 		}
/* 340 */ 	});	
/* 341 */ }
/* 342 */ 
/* 343 */ function getCurrentSlideName(){
/* 344 */ 	return $('body').attr('data-slide-name');
