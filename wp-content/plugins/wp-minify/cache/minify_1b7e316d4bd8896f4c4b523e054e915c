/* 108 */ 			if (format == 'num-to-month'){
/* 109 */ 				var val = $(this).html();
/* 110 */ 				var m = val-1;
/* 111 */ 				var month = months[m];
/* 112 */ 				$(this).html(month);
/* 113 */ 			}
