/* 465 */ function roundTo(num, place){
/* 466 */ 	var nn = Math.round(num/place);
/* 467 */ 	return nn * place;
/* 468 */ }
/* 469 */ 
/* 470 */ function addCommas(nStr){
/* 471 */ 	nStr += '';
/* 472 */ 	x = nStr.split('.');
/* 473 */ 	x1 = x[0];
/* 474 */ 	x2 = x.length > 1 ? '.' + x[1] : '';
/* 475 */ 	var rgx = /(\d+)(\d{3})/;
/* 476 */ 	while (rgx.test(x1)) {
/* 477 */ 		x1 = x1.replace(rgx, '$1' + ',' + '$2');
/* 478 */ 	}
/* 479 */ 	return x1 + x2;
/* 480 */ }
/* 481 */ 
/* 482 */ function toNumber(val){
/* 483 */ 	var str = String(val);
/* 484 */ 	str = str.replace(/[^\d.-]/g, "");
/* 485 */ 	var num = Number(str);
/* 486 */ 	return num;
/* 487 */ }
/* 488 */ 
/* 489 */ function toLower(str){
/* 490 */ 	if (str){
/* 491 */ 		return str.toLowerCase();
/* 492 */ 	}
/* 493 */ 	return str;
/* 494 */ }
/* 495 */ 
/* 496 */ function toBoolean(val){
/* 497 */ 	if (val == 0 || val == false || val == '0' || val == 'false' || val == ''){
/* 498 */ 		return false;
/* 499 */ 	} else {
/* 500 */ 		return true;

/* shebang.js */

/* 501 */ 	}
/* 502 */ }
