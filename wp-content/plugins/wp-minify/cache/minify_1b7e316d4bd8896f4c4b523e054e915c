/* 11  */     },this));
/* 12  */ 
/* 13  */     $.extend( this, options );
/* 14  */     this.setupInputs();
/* 15  */     this.setupFurnitureEvents();
/* 16  */     this.setMortgages();
/* 17  */   }
/* 18  */ 
/* 19  */   RefiAnalyzer.prototype.mortgages = {};
/* 20  */ 
/* 21  */   RefiAnalyzer.prototype.elements = {};
/* 22  */ 
/* 23  */   RefiAnalyzer.prototype.parameters = {
/* 24  */     mortgage_amount_remaining: 200000,
/* 25  */     mortgage_holding_period_years: 10,
/* 26  */     current_mortgage_payment: 1200,
/* 27  */     home_value: 200000,
/* 28  */     marginal_tax_rate: 25,
/* 29  */     itemize_flag: 1,
/* 30  */     mortgage_discount_points_fixed: 0,
/* 31  */     mortgage_discount_points_arm: 0,
/* 32  */     current_mortgage_type: "30_fixed",
/* 33  */     refi_mortgage_type: "71_arm",
/* 34  */     closing_cost_fixed_amount: 2500,
/* 35  */     get_cash_out_impact: false
/* 36  */   };
