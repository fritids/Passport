/* 189 */ 	function fixCalculators(){
/* 190 */ 		if ($('.excel-upload-output').length){
/* 191 */ 			
/* 192 */ 			$('.excel-upload-output tr').each(function(i){
/* 193 */ 				
/* 194 */ 				var tr = $(this);
/* 195 */ 				if (!tr.text().length && i == 0){
/* 196 */ 					tr.remove();
/* 197 */ 				}
/* 198 */ 				var rowClass = getAndDestroyClassTD(tr);
/* 199 */ 				
/* 200 */ 				assignClassToRow(tr, rowClass);

/* shebang-excel-upload.js */

/* 201 */ 				
/* 202 */ 				assignClassToCells(tr);
/* 203 */ 				var td = tr.find('td').not(':first');
/* 204 */ 				td.each(function(){
/* 205 */ 					var input = $(this).find('input');
/* 206 */ 					if (!input.length){
/* 207 */ 						$(this).addClass('excel-calc-label-small');
/* 208 */ 					}
/* 209 */ 				});
/* 210 */ 				
/* 211 */ 			});
/* 212 */ 			
/* 213 */ 			
/* 214 */ 			$('.excel-upload-output tr').css('height', 'auto');
/* 215 */ 			//$('[class*="input"], [class*="total"]').find('input[readonly="readonly"]').addClass('excel-calc-result').closest('tr').addClass('excel-output-result');
/* 216 */ 			$('[class*="input"], [class*="total"]').find('input[readonly="readonly"]').addClass('excel-calc-result');
/* 217 */ 			$('input').not('[readonly="readonly"]').addClass('excel-calc-input');
/* 218 */ 			$('.excel-calc-input').addClass('text-input-large-dollar');
/* 219 */ 			$('.excel-upload-output table').css('width', 'auto');
/* 220 */ 			$('.excel-upload-output table tr td span').css('display', 'inline');
/* 221 */ 			$('.excel-upload-output table').each(function(){
/* 222 */ 				fixCalculatorDataRows($(this));
/* 223 */ 				
/* 224 */ 				fixCalculatorText($(this));
/* 225 */ 				fixCalculatorMarkers($(this));
/* 226 */ 			});
/* 227 */ 			try {
/* 228 */ 				articleNavigator.refresh();
/* 229 */ 			} catch(e){
/* 230 */ 				trace('no articleNavigator found');
/* 231 */ 			}
/* 232 */ 		}
/* 233 */ 	}
