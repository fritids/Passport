/* 435 */ function shebangInstantSave(){
/* 436 */ 	var input = $(this);
/* 437 */ 	var field = input.attr('data-field');
/* 438 */ 	var id = input.attr('id');
/* 439 */ 	if (!field){
/* 440 */ 		field = id;
/* 441 */ 	}
/* 442 */ 	var val = input.val();
/* 443 */ 	if (input.is(':checkbox')){
/* 444 */ 		if (input.is(':checked')){
/* 445 */ 			val = 'on';
/* 446 */ 		} else {
/* 447 */ 			val = 0;
/* 448 */ 		}
/* 449 */ 	}
/* 450 */ 	$.totalStorage(field, val);

/* shebang.js */

/* 451 */ }
