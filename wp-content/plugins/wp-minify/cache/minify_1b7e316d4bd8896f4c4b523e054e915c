/* ari.js */

/* 1  */ var articleNavigator;
/* 2  */ 
/* 3  */ $(document).ready(function() {
/* 4  */ 	
/* 5  */ 	function ArticleNavigator() {
/* 6  */ 		this.refresh();
/* 7  */ 	}
/* 8  */ 
/* 9  */ 	$('#fixed-article-body-nav').localScroll({duration: 1000, onAfter: this.resetClicked});
/* 10 */ 	var wasClicked = false;
/* 11 */ 	
/* 12 */ 	ArticleNavigator.prototype.resetClicked = function() {
/* 13 */ 		wasClicked = false;
/* 14 */ 	}
/* 15 */ 	
/* 16 */ 	$(".article-body-nav-item").live('click', function() {
/* 17 */ 		if ($(this).hasClass('item-active')){
/* 18 */ 		} else {
/* 19 */ 			wasClicked = true;
/* 20 */ 			$(".article-body-nav").children().removeClass("item-active");
/* 21 */ 			$(this).addClass("item-active");
/* 22 */ 		}
/* 23 */ 	});
/* 24 */ 	
/* 25 */ 	ArticleNavigator.prototype.refresh = function(){
/* 26 */ 		if ($("#fixed-article-body-nav").is('*')) {
/* 27 */ 			// only do this if not ie7
/* 28 */ 			if (window.pageYOffset != undefined) {
/* 29 */ 				slideArray = new Array();
/* 30 */ 				
/* 31 */ 				$('.ui-article-block, tr.excel-output-header-marker').each(function(e) {
/* 32 */ 					var offset = $(this).offset().top - ($(window).height() * 0.2);
/* 33 */ 					var offsetBottom = $(this).height() + offset;
/* 34 */ 					var id = $(this).attr('id');
/* 35 */ 					if (!id){
/* 36 */ 						var newID = $(this).text();
/* 37 */ 						$(this).attr('id', newID);
/* 38 */ 						id = newID;
/* 39 */ 					}
/* 40 */ 					id = '#' + id;
/* 41 */ 					var itemArray = new Array(offset, offsetBottom, id);
/* 42 */ 					slideArray.push(itemArray);
/* 43 */ 				});
/* 44 */ 								
/* 45 */ 				$(window).scroll(function() {
/* 46 */ 					if (wasClicked == false) {
/* 47 */ 						var scrollPos = $(window).scrollTop();
/* 48 */ 						if (scrollPos == 0) {
/* 49 */ 							scrollPos = document.documentElement.scrollTop;
/* 50 */ 						}

/* ari.js */

/* 51 */ 						var docHeight = $(document).height();
/* 52 */ 						retarget();
/* 53 */ 					}
/* 54 */ 				});
/* 55 */ 			}
/* 56 */ 		}
/* 57 */ 	};
/* 58 */ 	
/* 59 */ 	function retarget() {
/* 60 */ 		var currentIndex;
/* 61 */ 		var scrollPos = $(window).scrollTop();
/* 62 */ 		if (scrollPos == 0) {
/* 63 */ 			scrollPos = document.documentElement.scrollTop;
/* 64 */ 		}
/* 65 */ 		slideSize = slideArray.length;
/* 66 */ 		
/* 67 */ 		// condition for if you are above the top item
/* 68 */ 		if (scrollPos < slideArray[0][0]) {
/* 69 */ 			$('.item-active').removeClass('item-active');
/* 70 */ 			$('.article-body-nav-item:eq(0)').addClass('item-active');
/* 71 */ 			
/* 72 */ 		// condition for if you're on the last item
/* 73 */ 		} else if (scrollPos > slideArray[slideSize-1][0]) {
/* 74 */ 			$('.item-active').removeClass('item-active');
/* 75 */ 			$('.article-body-nav-item:last').addClass('item-active');
/* 76 */ 			
/* 77 */ 		} else {
/* 78 */ 			// find current item's index
/* 79 */ 			for (var i=0; i<slideArray.length; i++) {
/* 80 */ 				if (scrollPos >= slideArray[i][0] && scrollPos < slideArray[i+1][0]) {
/* 81 */ 					currentIndex = i;
/* 82 */ 					break;
/* 83 */ 				}
/* 84 */ 			}
/* 85 */ 			$('.item-active').removeClass('item-active');
/* 86 */ 			$('.article-body-nav-item:eq('+currentIndex+')').addClass('item-active');
/* 87 */ 		}
/* 88 */ 		$('.navlink').show();
/* 89 */ 		//console.log('done');
/* 90 */ 	}
/* 91 */ 	
/* 92 */ 	articleNavigator = new ArticleNavigator();
/* 93 */ });

