/* 55  */ 	function randomString(len) {
/* 56  */ 		var chars = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXTZabcdefghiklmnopqrstuvwxyz";
/* 57  */ 		var randomstring = '';
/* 58  */ 		for (var i=0; i<len; i++) {
/* 59  */ 			var rnum = Math.floor(Math.random() * chars.length);
/* 60  */ 			randomstring += chars.substring(rnum,rnum+1);
/* 61  */ 		}
/* 62  */ 		return randomstring;
/* 63  */ 	}	
