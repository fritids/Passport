/* 89  */ 	function setPctsFromAmts(){
/* 90  */ 		$('.pct-from-amt').each(function(){
/* 91  */ 			var input = $(this);
/* 92  */ 			var num = shebangGetValue(input.data('num'));
/* 93  */ 			if (num == null){
/* 94  */ 				num = $('#'+input.data('num')).val();
/* 95  */ 			}
/* 96  */ 			var den = shebangGetValue(input.data('den'));
/* 97  */ 			if (den == null){
/* 98  */ 				den = $('#'+input.data('den')).val();
/* 99  */ 			}
/* 100 */ 			var pct = num / den;

/* shebang.js */

/* 101 */ 			input.val(roundTo(pct * 100, 1));
/* 102 */ 		});
/* 103 */ 	}
