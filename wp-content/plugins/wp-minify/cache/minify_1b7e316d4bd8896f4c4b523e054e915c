/* 55  */ function initAutoComplete(){
/* 56  */ 	
/* 57  */ 	try {
/* 58  */ 		$('.search-large').autocomplete({
/* 59  */ 			source:getAutoCompleteSource, 
/* 60  */ 			minLength:3,
/* 61  */ 			select: function( event, ui ) {
/* 62  */ 				window.location.href = ui.item.permalink;
/* 63  */ 				return false;
/* 64  */ 			}
/* 65  */ 		}).data('autocomplete')._renderItem = function(ul, item){
/* 66  */ 			ul.attr('id', 'search-dropdown-large');
/* 67  */ 			ul.addClass('search-dropdown-large');
/* 68  */ 			
/* 69  */ 			// de-slugify post type & force initial caps
/* 70  */ 			var newlabel = item.label.replace('-', ' ');
/* 71  */ 			newlabel = newlabel.replace(/^(.)|\s(.)/g, function($1){ return $1.toUpperCase(); });
/* 72  */ 			
/* 73  */ 			return $('<li class="search-result-li"></li>')
/* 74  */ 			.data( "item.autocomplete", item )
/* 75  */ 				.append( '<a href="'+item.permalink+'" class="search-result">' +
/* 76  */ 				         '<img class="search-result-img" src="'+item.thumb+'" />' +
/* 77  */ 				         '<span class="search-result-h" href="#">'+item.title+'</span>' + 
/* 78  */ 				         '<span class="subheader-tag '+item.label+'-tag">'+newlabel+'</span>' +
/* 79  */ 				         '<p class="search-result-description">'+item.excerpt+'</p></a>' )
/* 80  */ 				.appendTo( ul );
/* 81  */ 		};
/* 82  */ 		$('.search-large').bind('autocompletefocus', onAutoCompleteFocus);
/* 83  */ 	} catch(e){
/* 84  */ 		trace('auto complete did not load');
/* 85  */ 	}
/* 86  */ }
/* 87  */ 
/* 88  */ function getAutoCompleteSource(request, response){
/* 89  */    $.get('/wp-content/themes/shebang/search/search_instant.php', {'q': request.term}, function(data) {
/* 90  */       var obj = jQuery.parseJSON(data);
/* 91  */       response($.map(obj, function(item){
/* 92  */          return {
/* 93  */             label:item['post_type'],
/* 94  */             excerpt:item['excerpt'],
/* 95  */             title:item['title'],
/* 96  */             thumb:item['thumb_src'],
/* 97  */             permalink:item['permalink']
/* 98  */          }
/* 99  */       }));
/* 100 */    });

/* shebang-site.js */

/* 101 */ }
