/* 141 */ function initHomePageSlider(){
/* 142 */ 	$('.hp-billboard-slider article').css('float', 'left');
/* 143 */ 	$('.hp-billboard-slider article').css('width', '960px');
/* 144 */ 	$('.hp-billboard-slider article').wrapAll('<div class="items"></div>');
/* 145 */ 	$('.hp-billboard-slider').css('position', 'relative');
/* 146 */ 	$('.hp-billboard-slider').css('overflow', 'hidden');
/* 147 */ 	$('.hp-billboard-slider').css('padding-top', '0px');
/* 148 */ 	$('.hp-billboard-slider').css('top', '150px');
/* 149 */ 	$('.hp-billboard-slider .items').css('width', '20000em');
/* 150 */ 	$('.hp-billboard-slider .items').css('position', 'absolute');

/* shebang-site.js */

/* 151 */ 	var scroll = $('.hp-billboard-slider').scrollable({keyboard:false, onBeforeSeek:moveHomepageArrow, api:true, onSeek:onHomepageSeek});
/* 152 */ 	var hiConfig = {
/* 153 */ 		over:function(){
/* 154 */ 			var index = $(this).find('a').data('slide');
/* 155 */ 			scroll.seekTo(index);
/* 156 */ 		},
/* 157 */ 		out:function(){},
/* 158 */ 		interval:60,
/* 159 */ 		};
/* 160 */ 	$('.hp-billboard-nav-item').hoverIntent(hiConfig);
/* 161 */ 	$('.hp-billboard-nav-item').eq(0).addClass('nav-active');
/* 162 */ 	$('.hp-billboard-next-btn, .hp-billboard-prev-btn').click(function(e){
/* 163 */ 		e.preventDefault();
/* 164 */ 	});
/* 165 */ }
/* 166 */ 
/* 167 */ function onHomepageSeek(event, index){
/* 168 */ }
/* 169 */ 
/* 170 */ function moveHomepageArrow(event, index){
/* 171 */ 	var scroll = $('.hp-billboard-slider').data('scrollable');
/* 172 */ 	var currentIndex = scroll.getIndex();
/* 173 */ 	if (currentIndex == index){
/* 174 */ 		return;
/* 175 */ 	}
/* 176 */ 	var li = $('.hp-billboard-nav-item').eq(index);
/* 177 */ 	var left = '15%';
/* 178 */ 	if (index == 1){
/* 179 */ 		left = '48%';
/* 180 */ 	}
/* 181 */ 	if (index == 2){
/* 182 */ 		left = '81.75%';
/* 183 */ 	}
/* 184 */ 	$('.hp-billboard-nav-item').removeClass('nav-active');
/* 185 */ 	li.addClass('nav-active');
/* 186 */ 	$('.hp-billboard-active-arrow-group').css('left', left);
/* 187 */ 	/* Homepage Dollar Sign */
/* 188 */ 	var j = index + 1;
/* 189 */ 	if (j < 4){
/* 190 */ 		$('.hp-billboard-dollar-img').removeClass('slide-1 slide-2 slide-3');
/* 191 */ 		$('.hp-billboard-dollar-img').addClass('slide-'+j);
/* 192 */ 	}
/* 193 */ }
