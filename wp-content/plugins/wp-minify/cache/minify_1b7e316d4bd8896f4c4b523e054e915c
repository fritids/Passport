/* 243 */ 		var sel = '.'+target + ' a';
/* 244 */ 		$('.qs-sort-container').quicksand($(sel), {attribute:'data-id', adjustHeight:false}, onQuicksandComplete);
/* 245 */ 		var li = $(this).closest('li');
/* 246 */ 		li.siblings().removeClass('filter-active');
/* 247 */ 		li.addClass('filter-active');
/* 248 */ 	});
/* 249 */ }
