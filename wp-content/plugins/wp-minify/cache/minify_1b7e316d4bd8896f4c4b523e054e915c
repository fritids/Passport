/* 116 */ 	
/* 117 */ 	function setDefaultsToLocalStorage(){
/* 118 */ 		if (!$('#shebang_defaults').length){
/* 119 */ 			return;
/* 120 */ 		}
/* 121 */ 		var json = $('#shebang_defaults').text();
/* 122 */ 		if (!json){
/* 123 */ 			return;
/* 124 */ 		}
/* 125 */ 		var def = JSON.parse(json);
/* 126 */ 		for (var i = 0; i<def.length; i++){
/* 127 */ 			var row = def[i];
/* 128 */ 			var val = shebangGetValue(row.c_key);
/* 129 */ 			if (val == null){
/* 130 */ 				shebangSaveValue(row.c_key, row.c_value);
/* 131 */ 			}
/* 132 */ 		}
/* 133 */ 	}
/* 134 */ 	
/* 135 */ 	function setBooleanDisplayFromLocalStorage(){
/* 136 */ 		//for conditional display
/* 137 */ 		$('.ls-boolean-display').each(function(){
/* 138 */ 			var elm = $(this);
/* 139 */ 			elm.hide();
/* 140 */ 			var field = elm.attr('data-field');
/* 141 */ 			if (shebangGetValue(field)){
/* 142 */ 				elm.show();
/* 143 */ 			}
/* 144 */ 		});
/* 145 */ 		
/* 146 */ 		$('.ls-boolean-remove').each(function(){
/* 147 */ 			var elm = $(this);
/* 148 */ 			elm.hide();
/* 149 */ 			var field = elm.attr('data-field');
/* 150 */ 			if (shebangGetValue(field)){

/* shebang.js */

/* 151 */ 				elm.show();
/* 152 */ 			} else {
/* 153 */ 				elm.remove();
/* 154 */ 			}
/* 155 */ 		});
/* 156 */ 		
/* 157 */ 	}
/* 158 */ 	
/* 159 */ 	function setValuesFromTotalStorage(){
/* 160 */ 		$('.ls-value').each(function(){
/* 161 */ 			var elm = $(this);
/* 162 */ 			
/* 163 */ 			var field = elm.attr('data-field');
/* 164 */ 			if (!field){
/* 165 */ 				field = elm.attr('id');
/* 166 */ 			}
/* 167 */ 			var val = $.totalStorage(field);
/* 168 */ 			if (val == null){
/* 169 */ 				var fallback = elm.data('field-fallback');
/* 170 */ 				val = $.totalStorage(fallback);
/* 171 */ 			}
/* 172 */ 			if (!val && val != 0){
/* 173 */ 				return;
/* 174 */ 			}
/* 175 */ 			if (elm.is(':radio')){
/* 176 */ 				$('input[data-fiedl="'+field+'"]').removeAttr('checked');
/* 177 */ 				var sel = 'input[data-field="'+field+'"][value="'+val+'"]';
/* 178 */ 				var obj = $(sel);
/* 179 */ 				if (!obj.length){
/* 180 */ 					sel = 'input[id="'+field+'"][value="'+val+'"]';
/* 181 */ 					obj = $(sel);
/* 182 */ 				}
/* 183 */ 				obj.attr('checked', 'true');
/* 184 */ 			} else if (elm.is(':checkbox')){
/* 185 */ 				trace(field);
/* 186 */ 				if (val){
/* 187 */ 					elm.attr('checked', true);
/* 188 */ 				} else {
/* 189 */ 					elm.removeAttr('checked');
/* 190 */ 				}
/* 191 */ 			} else if (elm.is('input')){
/* 192 */ 				elm.val(val);
/* 193 */ 			} else if (elm.is('select')){
/* 194 */ 				elm.val(val);
/* 195 */ 				elm.find('option').removeAttr('selected')
/* 196 */ 				elm.find('option[value="'+val+'"]').attr('selected', 'selected');
/* 197 */ 			} else {
/* 198 */ 				if(toNumber(val) == val){
/* 199 */ 					if (elm.hasClass('round-to-10')){
/* 200 */ 						val = roundTo(val, 10);

/* shebang.js */

/* 201 */ 					}
/* 202 */ 					if (elm.hasClass('round-to-100')){
/* 203 */ 						val = roundTo(val, 100);
/* 204 */ 					}
/* 205 */ 					if (!elm.hasClass('no-commas')){
/* 206 */ 						val = addCommas(val);
/* 207 */ 					}
/* 208 */ 					elm.html(val);
/* 209 */ 				} else {
/* 210 */ 					elm.html(val);
/* 211 */ 				}
/* 212 */ 			}
/* 213 */ 		});
/* 214 */ 	}
