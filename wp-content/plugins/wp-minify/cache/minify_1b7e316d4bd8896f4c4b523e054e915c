/* 114 */ 					tr.addClass('open-group');
/* 115 */ 				}
/* 116 */ 				if (!tr.next(test).length){
/* 117 */ 					tr.addClass('close-group');
/* 118 */ 				}
/* 119 */ 			} else {
/* 120 */ 			}
/* 121 */ 		}); 
/* 122 */ 	}
