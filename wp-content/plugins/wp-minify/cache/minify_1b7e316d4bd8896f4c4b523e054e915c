/* 296 */ function getGraphUnit(maxVal){
/* 297 */ 	var maxh = toNumber($('.graph-container').css('max-height'));
/* 298 */ 	var unit = maxh / maxVal;
/* 299 */ 	return unit;
/* 300 */ }

