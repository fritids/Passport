/* 124 */ 	function fixCalculatorMarkers(table){
/* 125 */ 		$('.article-body-nav li.article-body-nav-item').not('.dynamic').addClass('flat');
/* 126 */ 		table.find('[class*="marker"]').each(function(){
/* 127 */ 			var tr = $(this);
/* 128 */ 			var gap = $('<tr class="gap before-marker"><td colspan="10"></td></tr>');
/* 129 */ 			tr.before(gap);
/* 130 */ 			var title = $.trim(tr.text());
/* 131 */ 			tr.attr('id', title);
/* 132 */ 			var index = $('.article-body-nav').children().length + 1;
/* 133 */ 			tr.attr('data-index', index);
/* 134 */ 			tr.addClass('dynamic-header-marker');
/* 135 */ 			var li = $('<li id="article-body-nav-item-'+index+'" class="article-body-nav-item dynamic"> <a href="#'+title+'">'+title+'</a></li>');
/* 136 */ 			var parentID = tr.closest('.ui-article-block').attr('id');
/* 137 */ 			var starter = $('.article-body-nav a[href="#'+parentID+'"]').closest('li');
/* 138 */ 			if (starter.next('.dynamic').length){
/* 139 */ 				//our starter has a dynamic after it
/* 140 */ 				var dynamics = starter.nextUntil('.flat');
/* 141 */ 				trace(dynamics);
/* 142 */ 				starter = dynamics.last();
/* 143 */ 			}
/* 144 */ 			starter.after(li);
/* 145 */ 		});
/* 146 */ 	}	
