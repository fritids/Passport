/* 90  */ 			var p = $('<p></p>');
/* 91  */ 			if (tr.hasClass('excel-output-header-marker')){
/* 92  */ 				p = $('<div></div>');
/* 93  */ 			}
/* 94  */ 			var id = randomString(5);
/* 95  */ 			input.attr('id', input.attr('name'));
/* 96  */ 			p.text(text);
/* 97  */ 			p.addClass('content');
/* 98  */ 			p.attr('data-input', input.attr('name'));
/* 99  */ 			p.attr('name', input.attr('name'));
/* 100 */ 			input.addClass('trigger-update-paragraph');

/* shebang-excel-upload.js */

/* 101 */ 			input.after(p);
/* 102 */ 			input.hide();
/* 103 */ 		});
