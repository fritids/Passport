/* 346 */ 
/* 347 */ function redrawBarGraphBar(htmlID, value, maxVal){
/* 348 */ 	var sel = '#graph-'+htmlID;
/* 349 */ 	var obj = $(sel);
/* 350 */ 	if (!obj.length){

/* shebang.js */

/* 351 */ 		obj = $('#'+htmlID);
/* 352 */ 	}
/* 353 */ 	obj.data('value', toNumber(value));
/* 354 */ 	var th = value * getGraphUnit(maxVal);
/* 355 */ 	if (th < 18){
/* 356 */ 		obj.addClass('too-small-bar');
/* 357 */ 	} else {
/* 358 */ 		obj.removeClass('too-small-bar');
/* 359 */ 	}
/* 360 */ 	
/* 361 */ 	if (th > 0){
/* 362 */ 		if (!obj.is(':visible')){
/* 363 */ 			obj.height(0);
/* 364 */ 		}
/* 365 */ 		obj.show();
/* 366 */ 		obj.height(th);
/* 367 */ 		var to = obj.data('timeout');
/* 368 */ 		clearTimeout(to);
/* 369 */ 	} else {
/* 370 */ 		hideBarGraphBar(htmlID);
/* 371 */ 	}
/* 372 */ 	obj.data('height', th);
/* 373 */ }
/* 374 */ 
/* 375 */ function getBarGraphValue(htmlID){
/* 376 */ 	var sel = '#graph-'+htmlID;
/* 377 */ 	var obj = $(sel);
/* 378 */ 	if (!obj.length){
/* 379 */ 		obj = $('#'+htmlID);
/* 380 */ 	}
/* 381 */ 	return obj.data('value');
/* 382 */ }
/* 383 */ 
/* 384 */ function updateBarGraphBar(htmlID, value){
/* 385 */ 	var sel = '#graph-'+htmlID;
/* 386 */ 	var obj = $(sel);
/* 387 */ 	if (!obj.length){
/* 388 */ 		obj = $('#'+htmlID);
/* 389 */ 	}
/* 390 */ 	obj.find('.amount').html(''+addCommas(roundTo(value, 10)));
/* 391 */ 	obj.data('value', toNumber(value));
/* 392 */ 	if (value > 0){
/* 393 */ 		obj.fadeIn();
/* 394 */ 	}
/* 395 */ 	var id = $(htmlID);
/* 396 */ 	if (id.length){
/* 397 */ 		id.val(value);
/* 398 */ 	} else {
/* 399 */ 		var hid = $('<input type="hidden" />');
/* 400 */ 		hid.attr('id', htmlID);

/* shebang.js */

/* 401 */ 		hid.val(value);
/* 402 */ 		$('body').append(hid);
/* 403 */ 	}
/* 404 */ }
/* 405 */ 
/* 406 */ function hideBarGraphBar(htmlID){
/* 407 */ 	var sel = '#graph-'+htmlID;
/* 408 */ 	var obj = $(sel);
/* 409 */ 	if (!obj.length){
/* 410 */ 		obj = $('#'+htmlID);
/* 411 */ 	}
/* 412 */ 	if (obj.css('display') == 'none'){
/* 413 */ 		return;
/* 414 */ 	}
/* 415 */ 	obj.height(0);
/* 416 */ 	obj.attr('data-height', 0);
/* 417 */ 	obj.data('value', 0);
/* 418 */ 	obj.fadeOut();
/* 419 */ }
