/* jquery.count-to.js */

/* 1  */ (function($) {
/* 2  */     $.fn.countTo = function(options) {
/* 3  */         // merge the default plugin settings with the custom options
/* 4  */         if (parseInt(options + 1)){
/* 5  */         	var to = options;
/* 6  */         	options = new Object();
/* 7  */         	options.to = to;
/* 8  */         	options = $.extend({}, $.fn.countTo.defaults, options || {});
/* 9  */         } else {
/* 10 */         	options = $.extend({}, $.fn.countTo.defaults, options || {});
/* 11 */         }
/* 12 */ 		var countLimit = 1000;
/* 13 */ 		var c = 0;
/* 14 */         // how many times to update the value, and how much to increment the value on each update
/* 15 */         if (options.from == 'cv'){
/* 16 */         	options.from = parseFloat($(this).text().replace('$', '').replace(',','')) * 1;
/* 17 */         }
/* 18 */         var loops = Math.ceil(options.speed / options.refreshInterval),
/* 19 */             increment = (options.to - options.from) / loops;
/* 20 */         return $(this).each(function() {
/* 21 */             var _this = this,
/* 22 */                 loopCount = 0,
/* 23 */                 value = options.from,
/* 24 */                 interval = setInterval(updateTimer, options.refreshInterval);
/* 25 */ 			
/* 26 */ 		
/* 27 */             function updateTimer() {
/* 28 */             	c++;
/* 29 */             	if (c > countLimit){
/* 30 */             		return;
/* 31 */             	}
/* 32 */                 value += increment;
/* 33 */                 loopCount++;
/* 34 */                 var nt = value.toFixed(options.decimals);
/* 35 */ 				if (options.format && options.format != null){
/* 36 */ 					try {
/* 37 */ 						nt = $.formatNumber(nt, options.format);
/* 38 */ 					} catch(e){
/* 39 */ 						//trace('formatNumber not avaiable');
/* 40 */ 					}
/* 41 */ 				}
/* 42 */ 				if (nt == ''){
/* 43 */ 					nt = 0;
/* 44 */ 				}
/* 45 */ 				if (options.commas){
/* 46 */ 					nt = addCommas(nt);
/* 47 */ 				}
/* 48 */                 $(_this).text(nt);
/* 49 */ 
/* 50 */                 if (typeof(options.onUpdate) == 'function') {

/* jquery.count-to.js */

/* 51 */                     options.onUpdate.call(_this, value);
/* 52 */                 }
/* 53 */ 
/* 54 */                 if (loopCount >= loops) {
/* 55 */                     clearInterval(interval);
/* 56 */                     value = options.to;
/* 57 */ 
/* 58 */                     if (typeof(options.onComplete) == 'function') {
/* 59 */                         options.onComplete.call(_this, value);
/* 60 */                     }
/* 61 */                 }
/* 62 */             }
/* 63 */         });
/* 64 */     };
/* 65 */ 
/* 66 */     $.fn.countTo.defaults = {
/* 67 */         from: 'cv',  // the number the element should start at
/* 68 */         to: 100,  // the number the element should end at
/* 69 */         speed: 800,  // how long it should take to count between the target numbers
/* 70 */         refreshInterval: 20,  // how often the element should be updated
/* 71 */         decimals: 0,  // the number of decimal places to show
/* 72 */         onUpdate: null,  // callback method for every time the element is updated,
/* 73 */         onComplete: null,  // callback method for when the element finishes updating
/* 74 */         format:null, //number format to use
/* 75 */         commas:true
/* 76 */     };
/* 77 */     
/* 78 */     function addCommas(nStr){
/* 79 */ 		nStr += '';
/* 80 */ 		x = nStr.split('.');
/* 81 */ 		x1 = x[0];
/* 82 */ 		x2 = x.length > 1 ? '.' + x[1] : '';
/* 83 */ 		var rgx = /(\d+)(\d{3})/;
/* 84 */ 		while (rgx.test(x1)) {
/* 85 */ 			x1 = x1.replace(rgx, '$1' + ',' + '$2');
/* 86 */ 		}
/* 87 */ 		return x1 + x2;
/* 88 */ 	}
/* 89 */ 
/* 90 */ })(jQuery);

