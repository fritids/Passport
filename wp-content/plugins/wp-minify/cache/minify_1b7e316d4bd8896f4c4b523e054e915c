/* 21  */ 	function wrapNumbers(text){
/* 22  */ 		var ta = text.split(' ');
/* 23  */ 		for (var i = 0; i<ta.length; i++){
/* 24  */ 			var word = ta[i];
/* 25  */ 			if (word.match(/\d+/g)){
/* 26  */ 				word = '<strong class="excel-bold-number">'+word+'</strong>';
/* 27  */ 				ta[i] = word;
/* 28  */ 			}
/* 29  */ 		}
/* 30  */ 		return ta.join(' ');
/* 31  */ 	}
/* 32  */ 	
/* 33  */ 	function onCalcChange(e){
/* 34  */ 		$('.trigger-update-paragraph').each(function(i){
/* 35  */ 			var input = $(this);
/* 36  */ 			var name = $(this).attr('name');
/* 37  */ 			var p = $('.content[name="'+name+'"]');
/* 38  */ 			var text = input.val();
/* 39  */ 			text = wrapNumbers(text);
/* 40  */ 			p.html(text);
/* 41  */ 		});
/* 42  */ 		
/* 43  */ 		//update markers on left to text in body
/* 44  */ 		$('.dynamic-header-marker').each(function(i){
/* 45  */ 			var tr = $(this);
/* 46  */ 			var index = tr.data('index');
/* 47  */ 			var text = tr.text();
/* 48  */ 			var li = $('#article-body-nav-item-'+index);
/* 49  */ 			li.find('a').text(text);
/* 50  */ 		});

