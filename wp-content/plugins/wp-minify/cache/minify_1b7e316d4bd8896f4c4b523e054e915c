/* 168 */ 	function assignClassToCells(tr){
/* 169 */ 		tr.find('td').each(function(){
/* 170 */ 			var td = $(this);
/* 171 */ 			var text = $.trim(td.text().toLowerCase());
/* 172 */ 			if (text.indexOf('per') == 0){
/* 173 */ 				td.addClass('align-left');
/* 174 */ 			}
/* 175 */ 		});
/* 176 */ 	}
/* 177 */ 	
/* 178 */ 	function getAndDestroyClassTD(tr){
/* 179 */ 		var rowClass = 'gap';
/* 180 */ 		var td = tr.find('td').first();
/* 181 */ 		var tdText = $.trim(td.text());
/* 182 */ 		if (tdText){
/* 183 */ 			rowClass= tdText;			
/* 184 */ 		}
/* 185 */ 		td.remove();
/* 186 */ 		return rowClass;
/* 187 */ 	}
