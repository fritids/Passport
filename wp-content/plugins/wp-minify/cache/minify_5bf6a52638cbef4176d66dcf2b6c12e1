/* 42 */ 			$('#media-buttons a.thickbox#add_video span.desc').text('Add Video');
/* 43 */ 			$('#media-buttons a.thickbox#add_audio span.desc').text('Add Audio');
/* 44 */ 			$('#media-buttons a.thickbox#add_media').hide();
/* 45 */ 		}
