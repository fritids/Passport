/* 54  */ function initAutoComplete(){
/* 55  */ 	
/* 56  */ 	try {
/* 57  */ 		$('.search-large').autocomplete({
/* 58  */ 			source:getAutoCompleteSource, 
/* 59  */ 			minLength:3,
/* 60  */ 			select: function( event, ui ) {
/* 61  */ 				trace('select');
/* 62  */ 				//$( "#project" ).val( ui.item.label );
/* 63  */ 				//$( "#project-id" ).val( ui.item.value );
/* 64  */ 				//$( "#project-description" ).html( ui.item.desc );
/* 65  */ 				//$( "#project-icon" ).attr( "src", "images/" + ui.item.icon );
/* 66  */ 
/* 67  */ 				return false;
/* 68  */ 			}
/* 69  */ 		}).data('autocomplete')._renderItem = function(ul, item){
/* 70  */ 			ul.attr('id', 'search-dropdown-large');
/* 71  */ 			ul.addClass('search-dropdown-large');
/* 72  */ 			
/* 73  */ 			// de-slugify post type & force initial caps
/* 74  */ 			var newlabel = item.label.replace('-', ' ');
/* 75  */ 			newlabel = newlabel.replace(/^(.)|\s(.)/g, function($1){ return $1.toUpperCase(); });
/* 76  */ 			
/* 77  */ 			return $('<li class="search-result-li"></li>')
/* 78  */ 			.data( "item.autocomplete", item )
/* 79  */ 				.append( '<a href="'+item.permalink+'" class="search-result">' +
/* 80  */ 				         '<img class="search-result-img" src="'+item.thumb+'" />' +
/* 81  */ 				         '<span class="search-result-h" href="#">'+item.title+'</span>' + 
/* 82  */ 				         '<span class="subheader-tag '+item.label+'-tag">'+newlabel+'</span>' +
/* 83  */ 				         '<p class="search-result-description">'+item.excerpt+'</p></a>' )
/* 84  */ 				.appendTo( ul );
/* 85  */ 		};
/* 86  */ 		$('.search-large').bind('autocompletefocus', onAutoCompleteFocus);
/* 87  */ 	} catch(e){
/* 88  */ 		trace('auto complete did not load');
/* 89  */ 	}
/* 90  */ }
