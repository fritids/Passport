/* 332 */ 
/* 333 */ function redrawBarGraphBar(htmlID, value, maxVal){
/* 334 */ 	var sel = '#graph-'+htmlID;
/* 335 */ 	var obj = $(sel);
/* 336 */ 	if (!obj.length){
/* 337 */ 		obj = $('#'+htmlID);
/* 338 */ 	}
/* 339 */ 	obj.data('value', toNumber(value));
/* 340 */ 	var th = value * getGraphUnit(maxVal);
/* 341 */ 	if (th < 18){
/* 342 */ 		obj.addClass('too-small-bar');
/* 343 */ 	} else {
/* 344 */ 		obj.removeClass('too-small-bar');
/* 345 */ 	}
/* 346 */ 	
/* 347 */ 	if (th > 0){
/* 348 */ 		if (!obj.is(':visible')){
/* 349 */ 			obj.height(0);
/* 350 */ 		}

/* shebang.js */

/* 351 */ 		obj.show();
/* 352 */ 		obj.height(th);
/* 353 */ 		var to = obj.data('timeout');
/* 354 */ 		clearTimeout(to);
/* 355 */ 	} else {
/* 356 */ 		hideBarGraphBar(htmlID);
/* 357 */ 	}
/* 358 */ 	obj.data('height', th);
/* 359 */ }
/* 360 */ 
/* 361 */ function getBarGraphValue(htmlID){
/* 362 */ 	var sel = '#graph-'+htmlID;
/* 363 */ 	var obj = $(sel);
/* 364 */ 	if (!obj.length){
/* 365 */ 		obj = $('#'+htmlID);
/* 366 */ 	}
/* 367 */ 	return obj.data('value');
/* 368 */ }
/* 369 */ 
/* 370 */ function updateBarGraphBar(htmlID, value){
/* 371 */ 	var sel = '#graph-'+htmlID;
/* 372 */ 	var obj = $(sel);
/* 373 */ 	if (!obj.length){
/* 374 */ 		obj = $('#'+htmlID);
/* 375 */ 	}
/* 376 */ 	obj.find('.amount').html(''+addCommas(roundTo(value, 10)));
/* 377 */ 	obj.data('value', toNumber(value));
/* 378 */ 	if (value > 0){
/* 379 */ 		obj.fadeIn();
/* 380 */ 	}
/* 381 */ 	var id = $(htmlID);
/* 382 */ 	if (id.length){
/* 383 */ 		id.val(value);
/* 384 */ 	} else {
/* 385 */ 		var hid = $('<input type="hidden" />');
/* 386 */ 		hid.attr('id', htmlID);
/* 387 */ 		hid.val(value);
/* 388 */ 		$('body').append(hid);
/* 389 */ 	}
/* 390 */ }
/* 391 */ 
/* 392 */ function hideBarGraphBar(htmlID){
/* 393 */ 	var sel = '#graph-'+htmlID;
/* 394 */ 	var obj = $(sel);
/* 395 */ 	if (!obj.length){
/* 396 */ 		obj = $('#'+htmlID);
/* 397 */ 	}
/* 398 */ 	if (obj.css('display') == 'none'){
/* 399 */ 		return;
/* 400 */ 	}

/* shebang.js */

/* 401 */ 	obj.height(0);
/* 402 */ 	obj.attr('data-height', 0);
/* 403 */ 	obj.data('value', 0);
/* 404 */ 	obj.fadeOut();
/* 405 */ }
