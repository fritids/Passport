/* 281 */ 
/* 282 */ function getGraphUnit(maxVal){
/* 283 */ 	var maxh = toNumber($('.graph-container').css('max-height'));
/* 284 */ 	var unit = maxh / maxVal;
/* 285 */ 	return unit;
/* 286 */ }
/* 287 */ 
/* 288 */ function getGraphMaxVal(){
/* 289 */ 	var max = 0;
/* 290 */ 	$('.bar-graph-bar').each(function(){
/* 291 */ 		var val = toNumber($(this).data('value'));
/* 292 */ 		if (val > 0){
/* 293 */ 			max += val; 
/* 294 */ 		}
/* 295 */ 	});
/* 296 */ 	return max;
/* 297 */ }
/* 298 */ 
/* 299 */ function redrawBarGraph(maxVal){
/* 300 */ 	if (!maxVal){

/* shebang.js */

/* 301 */ 		maxVal = getGraphMaxVal();
/* 302 */ 	}
/* 303 */ 	$('.bar-graph-bar').each(function(){
/* 304 */ 		var htmlID = $(this).attr('id');
/* 305 */ 		var val = $(this).find('.amount').html();
/* 306 */ 		if (val == '' || val == null){
/* 307 */ 			return;
/* 308 */ 		}
/* 309 */ 		if (val){
/* 310 */ 			val = toNumber(val.replace('$', ''));
/* 311 */ 		}
/* 312 */ 		if (val){
/* 313 */ 			updateBarGraphBar(htmlID, val);
/* 314 */ 		} else {
/* 315 */ 			hideBarGraphBar(htmlID);
/* 316 */ 		}
/* 317 */ 	});
/* 318 */ 	$('.bar-graph-bar').each(function(){
/* 319 */ 		var htmlID = $(this).attr('id');
/* 320 */ 		var val = $(this).data('value');
/* 321 */ 		if (val){
/* 322 */ 			redrawBarGraphBar(htmlID, val, maxVal);
/* 323 */ 		} else {
/* 324 */ 			hideBarGraphBar(htmlID);
/* 325 */ 		}
/* 326 */ 	});	
/* 327 */ }
