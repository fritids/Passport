/* 33  */ 	function fixCalculatorDataRows(table){
/* 34  */ 		var testClass = 'excel-output-data-input';
/* 35  */ 		var test = '[class*="input"]';
/* 36  */ 		table.find('tr').each(function(){
/* 37  */ 			var tr = $(this);
/* 38  */ 			if (tr.filter(test).length){
/* 39  */ 				if (!tr.prev(test).length){
/* 40  */ 					//its the first in row
/* 41  */ 					tr.addClass('open-group');
/* 42  */ 				}
/* 43  */ 				if (!tr.next(test).length){
/* 44  */ 					tr.addClass('close-group');
/* 45  */ 				}
/* 46  */ 			} else {
/* 47  */ 			}
/* 48  */ 		}); 
/* 49  */ 	}
/* 50  */ 	

/* shebang-excel-upload.js */

/* 51  */ 	function fixCalculatorMarkers(table){
/* 52  */ 		$('.article-body-nav li.article-body-nav-item').addClass('flat');
/* 53  */ 		table.find('[class*="marker"]').each(function(){
/* 54  */ 			var tr = $(this);
/* 55  */ 			var gap = $('<tr class="gap before-marker"><td colspan="10"></td></tr>');
/* 56  */ 			tr.before(gap);
/* 57  */ 			var title = tr.text();
/* 58  */ 			tr.attr('id', title);
/* 59  */ 			var index = $('.article-body-nav').children().length + 1;
/* 60  */ 			var li = $('<li id="article-body-nav-item-'+index+'" class="article-body-nav-item dynamic"> <a href="#'+title+'">'+title+'</a></li>');
/* 61  */ 			var parentID = tr.closest('.ui-article-block').attr('id');
/* 62  */ 			var starter = $('.article-body-nav a[href="#'+parentID+'"]').closest('li');
/* 63  */ 			if (starter.next('.dynamic').length){
/* 64  */ 				//our starter has a dynamic after it
/* 65  */ 				var dynamics = starter.nextUntil('.flat');
/* 66  */ 				trace(dynamics);
/* 67  */ 				starter = dynamics.last();
/* 68  */ 			}
/* 69  */ 			starter.after(li);
/* 70  */ 		});
/* 71  */ 	}	
/* 72  */ 	
/* 73  */ 	function assignClassToRow(tr, rowClass){
/* 74  */ 		tr.addClass('excel-output-'+rowClass);
/* 75  */ 		if (rowClass.indexOf('header') > -1 || rowClass.indexOf('text') > -1){
/* 76  */ 			//header row
/* 77  */ 			tr.find('td:gt(0)').remove();
/* 78  */ 			tr.find('td').first().attr('colspan', 10);
/* 79  */ 			tr.find('td').wrapInner('<div class="content"></div>');
/* 80  */ 		}
/* 81  */ 		
/* 82  */ 		if (rowClass == 'gap'){
/* 83  */ 			
/* 84  */ 		}
/* 85  */ 	}
/* 86  */ 	
/* 87  */ 	function assignClassToCells(tr){
/* 88  */ 		tr.find('td').each(function(){
/* 89  */ 			var td = $(this);
/* 90  */ 			var text = $.trim(td.text().toLowerCase());
/* 91  */ 			if (text.indexOf('per') == 0){
/* 92  */ 				td.addClass('align-left');
/* 93  */ 			}
/* 94  */ 		});
/* 95  */ 	}
/* 96  */ 	
/* 97  */ 	function getAndDestroyClassTD(tr){
/* 98  */ 		var rowClass = 'gap';
/* 99  */ 		var td = tr.find('td').first();
/* 100 */ 		var tdText = $.trim(td.text());

/* shebang-excel-upload.js */

/* 101 */ 		if (tdText){
/* 102 */ 			rowClass= tdText;			
/* 103 */ 		}
/* 104 */ 		td.remove();
/* 105 */ 		return rowClass;
/* 106 */ 	}
/* 107 */ 	
/* 108 */ 	function fixCalculators(){
/* 109 */ 		if ($('.excel-upload-output').length){
/* 110 */ 			$('.excel-upload-output tr').each(function(i){
/* 111 */ 				var tr = $(this);
/* 112 */ 				if (!tr.text().length && i == 0){
/* 113 */ 					tr.remove();
/* 114 */ 				}
/* 115 */ 				//fixCalculatorsIDRows(tr);
/* 116 */ 				var rowClass = getAndDestroyClassTD(tr);
/* 117 */ 				assignClassToRow(tr, rowClass);
/* 118 */ 				assignClassToCells(tr);
/* 119 */ 				var td = tr.find('td').not(':first');
/* 120 */ 				td.each(function(){
/* 121 */ 					var input = $(this).find('input');
/* 122 */ 					if (!input.length){
/* 123 */ 						$(this).addClass('excel-calc-label-small');
/* 124 */ 					}
/* 125 */ 				});
/* 126 */ 			});
/* 127 */ 			
/* 128 */ 			$('.excel-upload-output tr').css('height', 'auto');
/* 129 */ 			$('input[readonly="readonly"]').addClass('excel-calc-result').closest('tr').addClass('excel-output-result');
/* 130 */ 			$('input').not('[readonly="readonly"]').addClass('excel-calc-input');
/* 131 */ 			$('.excel-calc-input').addClass('text-input-large-dollar');
/* 132 */ 			$('.excel-upload-output table').css('width', 'auto');
/* 133 */ 			$('.excel-upload-output table tr td span').css('display', 'inline');
/* 134 */ 			$('.excel-upload-output table').each(function(){
/* 135 */ 				fixCalculatorDataRows($(this));
/* 136 */ 				fixCalculatorMarkers($(this));
/* 137 */ 				//fixCalculatorResultRows($(this));
/* 138 */ 			});
/* 139 */ 			articleNavigator.refresh();
/* 140 */ 		}
/* 141 */ 	}
/* 142 */ 	
/* 143 */ 	function cleanCalculators(){
/* 144 */ 		$('.excel-upload-output').each(function(){
/* 145 */ 			var html = $(this).html();
/* 146 */ 			$(this).html($.trim(html));
/* 147 */ 		});
/* 148 */ 	}
/* 149 */ 
/* 150 */ });

/* shebang-excel-upload.js */

