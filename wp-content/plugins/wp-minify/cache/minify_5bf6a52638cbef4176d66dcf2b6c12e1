/* 421 */ function shebangInstantSave(){
/* 422 */ 	var input = $(this);
/* 423 */ 	var field = input.attr('data-field');
/* 424 */ 	var id = input.attr('id');
/* 425 */ 	if (!field){
/* 426 */ 		field = id;
/* 427 */ 	}
/* 428 */ 	var val = input.val();
/* 429 */ 	if (input.is(':checkbox')){
/* 430 */ 		if (input.is(':checked')){
/* 431 */ 			val = 'on';
/* 432 */ 		} else {
/* 433 */ 			val = 0;
/* 434 */ 		}
/* 435 */ 	}
/* 436 */ 	$.totalStorage(field, val);
/* 437 */ }
