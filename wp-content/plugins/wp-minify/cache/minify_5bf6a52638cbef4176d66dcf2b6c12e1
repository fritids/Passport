/* 219 */ 		var p = $(this);
/* 220 */ 		var phtml = p.html();
/* 221 */ 		if (phtml){
/* 222 */ 			if (!p.html().length){
/* 223 */ 				
/* 224 */ 			}
/* 225 */ 			if (!phtml.trim().length){
/* 226 */ 				p.remove();
/* 227 */ 			}
/* 228 */ 			if(p.html().trim() == ''){
/* 229 */ 				p.remove();
/* 230 */ 			}
/* 231 */ 		}
/* 232 */ 	});
/* 233 */ 	
/* 234 */ 	/* Wrap <p>s in lists */
/* 235 */ 	$('.ui-article-block li').wrapInner('<p>');
/* 236 */ }
