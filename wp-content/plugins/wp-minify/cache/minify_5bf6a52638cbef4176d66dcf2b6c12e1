/* 244 */ 		if (!$(sel).length){
/* 245 */ 			$(this).slideUp();
/* 246 */ 		}
/* 247 */ 	});
/* 248 */ 	$('.trigger-qs').click(function(e){
/* 249 */ 		e.preventDefault();
/* 250 */ 		var target = $(this).data('target');

/* shebang-site.js */

/* 251 */ 		var sel = '.'+target + ' a';
/* 252 */ 		$('.qs-sort-container').quicksand($(sel), {attribute:'data-id', adjustHeight:false}, onQuicksandComplete);
/* 253 */ 		var li = $(this).closest('li');
/* 254 */ 		li.siblings().removeClass('filter-active');
/* 255 */ 		li.addClass('filter-active');
/* 256 */ 	});
/* 257 */ }
