/* 175 */ function onHomepageSeek(event, index){
/* 176 */ }
/* 177 */ 
/* 178 */ function moveHomepageArrow(event, index){
/* 179 */ 	var scroll = $('.hp-billboard-slider').data('scrollable');
/* 180 */ 	var currentIndex = scroll.getIndex();
/* 181 */ 	if (currentIndex == index){
/* 182 */ 		return;
/* 183 */ 	}
/* 184 */ 	var li = $('.hp-billboard-nav-item').eq(index);
/* 185 */ 	var left = '15%';
/* 186 */ 	if (index == 1){
/* 187 */ 		left = '48%';
/* 188 */ 	}
/* 189 */ 	if (index == 2){
/* 190 */ 		left = '81.75%';
/* 191 */ 	}
/* 192 */ 	$('.hp-billboard-nav-item').removeClass('nav-active');
/* 193 */ 	li.addClass('nav-active');
/* 194 */ 	$('.hp-billboard-active-arrow-group').css('left', left);
/* 195 */ 	/* Homepage Dollar Sign */
/* 196 */ 	var j = index + 1;
/* 197 */ 	if (j < 4){
/* 198 */ 		$('.hp-billboard-dollar-img').removeClass('slide-1 slide-2 slide-3');
/* 199 */ 		$('.hp-billboard-dollar-img').addClass('slide-'+j);
/* 200 */ 	}

/* shebang-site.js */

/* 201 */ }
