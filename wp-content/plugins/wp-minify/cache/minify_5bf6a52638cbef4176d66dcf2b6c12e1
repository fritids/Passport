/* 12 */ 		function fixViewButtons(){
/* 13 */ 			if ($('#post_parent').length && typenow != 'calculators'){
/* 14 */ 				var link = $('#view-post-btn a');
/* 15 */ 				var href = link.attr('href');
/* 16 */ 				var lastSlashIndex = href.lastIndexOf('/');
/* 17 */ 				var hStart = href.substr(0, lastSlashIndex);
/* 18 */ 				var pid = $('#post_ID').val();
/* 19 */ 				href = hStart + '#block-' + pid;
/* 20 */ 				link.attr('href', href);
/* 21 */ 			}
/* 22 */ 		}
/* 23 */ 		
/* 24 */ 		function fixForm(){
/* 25 */ 			$('form#post').attr('enctype', 'multipart/form-data');
/* 26 */ 		}
/* 27 */ 		
/* 28 */ 		function sortAdminMenu(){
/* 29 */ 			var links = $('#menu-links').first();
/* 30 */ 			links.detach();
/* 31 */ 			$('#menu-appearance').after(links);
/* 32 */ 			links.find('a.menu-top').text('Footer Links');
/* 33 */ 			
/* 34 */ 			var promo = $('#toplevel_page_promo-manager').first();
/* 35 */ 			promo.detach();
/* 36 */ 			$('#menu-posts-rollers').after(promo);
/* 37 */ 		}
/* 38 */ 		
