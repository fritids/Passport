/* 75  */ 	function setPctsFromAmts(){
/* 76  */ 		$('.pct-from-amt').each(function(){
/* 77  */ 			var input = $(this);
/* 78  */ 			var num = shebangGetValue(input.data('num'));
/* 79  */ 			if (num == null){
/* 80  */ 				num = $('#'+input.data('num')).val();
/* 81  */ 			}
/* 82  */ 			var den = shebangGetValue(input.data('den'));
/* 83  */ 			if (den == null){
/* 84  */ 				den = $('#'+input.data('den')).val();
/* 85  */ 			}
/* 86  */ 			var pct = num / den;
/* 87  */ 			input.val(roundTo(pct * 100, 1));
/* 88  */ 		});
/* 89  */ 	}
/* 90  */ 	
/* 91  */ 	function setFormatsFromTotalStorage(){
/* 92  */ 		$('.ls-format').each(function(){
/* 93  */ 			var format = $(this).data('format');
/* 94  */ 			if (format == 'num-to-month'){
/* 95  */ 				var val = $(this).html();
/* 96  */ 				var m = val-1;
/* 97  */ 				var month = months[m];
/* 98  */ 				$(this).html(month);
/* 99  */ 			}
/* 100 */ 		});

/* shebang.js */

/* 101 */ 	}
/* 102 */ 	
/* 103 */ 	function setDefaultsToLocalStorage(){
/* 104 */ 		if (!$('#shebang_defaults').length){
/* 105 */ 			return;
/* 106 */ 		}
/* 107 */ 		var json = $('#shebang_defaults').text();
/* 108 */ 		if (!json){
/* 109 */ 			return;
/* 110 */ 		}
/* 111 */ 		var def = JSON.parse(json);
/* 112 */ 		for (var i = 0; i<def.length; i++){
/* 113 */ 			var row = def[i];
/* 114 */ 			var val = shebangGetValue(row.c_key);
/* 115 */ 			if (val == null){
/* 116 */ 				shebangSaveValue(row.c_key, row.c_value);
/* 117 */ 			}
/* 118 */ 		}
/* 119 */ 	}
/* 120 */ 	
/* 121 */ 	function setBooleanDisplayFromLocalStorage(){
/* 122 */ 		//for conditional display
/* 123 */ 		$('.ls-boolean-display').each(function(){
/* 124 */ 			var elm = $(this);
/* 125 */ 			elm.hide();
/* 126 */ 			var field = elm.attr('data-field');
/* 127 */ 			if (shebangGetValue(field)){
/* 128 */ 				elm.show();
/* 129 */ 			}
/* 130 */ 		});
/* 131 */ 		
/* 132 */ 		$('.ls-boolean-remove').each(function(){
/* 133 */ 			var elm = $(this);
/* 134 */ 			elm.hide();
/* 135 */ 			var field = elm.attr('data-field');
/* 136 */ 			if (shebangGetValue(field)){
/* 137 */ 				elm.show();
/* 138 */ 			} else {
/* 139 */ 				elm.remove();
/* 140 */ 			}
/* 141 */ 		});
/* 142 */ 		
/* 143 */ 	}
/* 144 */ 	
/* 145 */ 	function setValuesFromTotalStorage(){
/* 146 */ 		$('.ls-value').each(function(){
/* 147 */ 			var elm = $(this);
/* 148 */ 			
/* 149 */ 			var field = elm.attr('data-field');
/* 150 */ 			if (!field){

/* shebang.js */

/* 151 */ 				field = elm.attr('id');
/* 152 */ 			}
/* 153 */ 			var val = $.totalStorage(field);
/* 154 */ 			if (val == null){
/* 155 */ 				var fallback = elm.data('field-fallback');
/* 156 */ 				val = $.totalStorage(fallback);
/* 157 */ 			}
/* 158 */ 			if (!val && val != 0){
/* 159 */ 				return;
/* 160 */ 			}
/* 161 */ 			if (elm.is(':radio')){
/* 162 */ 				$('input[data-fiedl="'+field+'"]').removeAttr('checked');
/* 163 */ 				var sel = 'input[data-field="'+field+'"][value="'+val+'"]';
/* 164 */ 				var obj = $(sel);
/* 165 */ 				if (!obj.length){
/* 166 */ 					sel = 'input[id="'+field+'"][value="'+val+'"]';
/* 167 */ 					obj = $(sel);
/* 168 */ 				}
/* 169 */ 				obj.attr('checked', 'true');
/* 170 */ 			} else if (elm.is(':checkbox')){
/* 171 */ 				trace(field);
/* 172 */ 				if (val){
/* 173 */ 					elm.attr('checked', true);
/* 174 */ 				} else {
/* 175 */ 					elm.removeAttr('checked');
/* 176 */ 				}
/* 177 */ 			} else if (elm.is('input')){
/* 178 */ 				elm.val(val);
/* 179 */ 			} else if (elm.is('select')){
/* 180 */ 				elm.val(val);
/* 181 */ 				elm.find('option').removeAttr('selected')
/* 182 */ 				elm.find('option[value="'+val+'"]').attr('selected', 'selected');
/* 183 */ 			} else {
/* 184 */ 				if(toNumber(val) == val){
/* 185 */ 					if (elm.hasClass('round-to-10')){
/* 186 */ 						val = roundTo(val, 10);
/* 187 */ 					}
/* 188 */ 					if (elm.hasClass('round-to-100')){
/* 189 */ 						val = roundTo(val, 100);
/* 190 */ 					}
/* 191 */ 					if (!elm.hasClass('no-commas')){
/* 192 */ 						val = addCommas(val);
/* 193 */ 					}
/* 194 */ 					elm.html(val);
/* 195 */ 				} else {
/* 196 */ 					elm.html(val);
/* 197 */ 				}
/* 198 */ 			}
/* 199 */ 		});
/* 200 */ 	}

/* shebang.js */

