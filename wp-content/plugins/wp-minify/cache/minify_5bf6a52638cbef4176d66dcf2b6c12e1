/* 152 */ 	
/* 153 */ 	function trace(msg){
/* 154 */ 		try{
/* 155 */ 			console.log(msg);
/* 156 */ 		} catch(e){}
/* 157 */ 	}
/* 158 */ /*
/* 159 *| ;(function($){
/* 160 *| 	$.fn.fixedScroll = function(opts){
/* 161 *| 	
/* 162 *| 		var defaults = {fixedClass:'fixed', topLimit:0, bottomLimit:-1, absClass:'absolute', offset:0};
/* 163 *| 		var opts = $.extend(defaults, opts);
/* 164 *| 		
/* 165 *| 		return this.each(function(){
/* 166 *| 			var item = $(this);
/* 167 *| 			var over;
/* 168 *| 			$(window).bind('scroll resize', function(){
/* 169 *| 				var wt = $(window).scrollTop();
/* 170 *| 				var gt = 0;
/* 171 *| 				if (item.data('oy')){
/* 172 *| 					gt = item.data('oy');
/* 173 *| 				} else {
/* 174 *| 					gt = item.offset().top;
/* 175 *| 					item.data('oy', gt);
/* 176 *| 					item.data('ow', item.width());
/* 177 *| 				}	
/* 178 *| 				var dist = gt - wt;
/* 179 *| 				item.css('top', '');
/* 180 *| 				if (dist < opts.topLimit){
/* 181 *| 					item.addClass(opts.fixedClass);
/* 182 *| 					item.removeClass(opts.absClass);
/* 183 *| 					item.css('top', opts.topLimit);
/* 184 *| 					item.width(item.data('ow'));
/* 185 *| 				} else {
/* 186 *| 					item.removeClass(opts.fixedClass);
/* 187 *| 					item.removeClass(opts.absClass);
/* 188 *| 				}
/* 189 *| 				if (opts.bottomLimit > -1 ){
/* 190 *| 					if (wt > opts.bottomLimit - item.outerHeight()){
/* 191 *| 						if (!over){
/* 192 *| 							over = wt;
/* 193 *| 						}
/* 194 *| 						item.removeClass(opts.fixedClass);
/* 195 *| 						item.addClass(opts.absClass);
/* 196 *| 						var top = over - opts.offset;
/* 197 *|  						item.css('top', top);
/* 198 *| 					}
/* 199 *| 				}
/* 200 *| 				

/* tito-base.js */

