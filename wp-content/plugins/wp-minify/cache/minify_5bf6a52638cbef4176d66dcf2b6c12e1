/* jquery.fixed-scroll.js */

/* 1  */ ;(function($){
/* 2  */ 	$.fn.fixedScroll = function(opts){
/* 3  */ 	
/* 4  */ 		var defaults = {fixedClass:'fixed', absClass:'relative', container:'parent', offset:0, forceTop:false};
/* 5  */ 		var opts = $.extend(defaults, opts);
/* 6  */ 		
/* 7  */ 		return this.each(function(){
/* 8  */ 			var item = $(this);
/* 9  */ 			var over;
/* 10 */ 			var width = $(this).width();
/* 11 */ 			var container = item.parent();
/* 12 */ 			if (opts.container != 'parent'){
/* 13 */ 				container = opts.container;
/* 14 */ 				if (typeof container == 'string'){
/* 15 */ 					container = $(container);
/* 16 */ 				}
/* 17 */ 			}
/* 18 */ 			var topLimit = item.offset().top - container.offset().top + opts.offset;
/* 19 */ 			$(window).bind('scroll resize', onScrollCheck);
/* 20 */ 			container.bind('resize', onScrollCheck);
/* 21 */ 			
/* 22 */ 			function onScrollCheck(){
/* 23 */ 				var wt = $(window).scrollTop();
/* 24 */ 				var gt = 0;
/* 25 */ 				if (item.data('oy')){
/* 26 */ 					gt = item.data('oy');
/* 27 */ 				} else {
/* 28 */ 					gt = item.offset().top;
/* 29 */ 					item.data('oy', gt);
/* 30 */ 				}	
/* 31 */ 				var dist = gt - wt;
/* 32 */ 				if (dist < topLimit){
/* 33 */ 					//going fixed
/* 34 */ 					item.addClass(opts.fixedClass);
/* 35 */ 					item.removeClass(opts.absClass);
/* 36 */ 					item.width(width);
/* 37 */ 				} else {
/* 38 */ 					//going relative
/* 39 */ 					item.removeClass(opts.fixedClass);
/* 40 */ 					item.removeClass(opts.absClass);
/* 41 */ 				}
/* 42 */ 				if (wt > container.offset().top + container.height() - item.outerHeight()){
/* 43 */ 					item.removeClass(opts.fixedClass);
/* 44 */ 					item.addClass(opts.absClass);
/* 45 */ 					var top = container.height() - item.outerHeight();
/* 46 */ 					item.css('top', top);
/* 47 */ 				} else {
/* 48 */ 					if (opts.forceTop){
/* 49 */ 						item.css('top', '0px');
/* 50 */ 					} else {

/* jquery.fixed-scroll.js */

/* 51 */ 						item.css('top', '');
/* 52 */ 					}
/* 53 */ 				}
/* 54 */ 			}
/* 55 */ 		});
/* 56 */ 	}
/* 57 */ })(jQuery);

;
/* jquery.tools.min.js */

/* 1 */ /* jQuery Tools 1.2.6 works with jQuery 1.6.4 */
