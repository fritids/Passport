/* 160 */ 	var hiConfig = {
/* 161 */ 		over:function(){
/* 162 */ 			var index = $(this).find('a').data('slide');
/* 163 */ 			scroll.seekTo(index);
/* 164 */ 		},
/* 165 */ 		out:function(){},
/* 166 */ 		interval:60,
/* 167 */ 		};
