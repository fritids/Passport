/* 92  */ function getAutoCompleteSource(request, response){
/* 93  */    $.get('/wp-content/themes/shebang/search/search_instant.php', {'q': request.term}, function(data) {
/* 94  */ 	  console.log('term:'+request.term);
/* 95  */ 	   console.log(data);
/* 96  */       var obj = jQuery.parseJSON(data);
/* 97  */       console.log(obj);
/* 98  */       response($.map(obj, function(item){
/* 99  */          console.log(item);
/* 100 */          return {

/* shebang-site.js */

/* 101 */             label:item['post_type'],
/* 102 */             excerpt:item['excerpt'],
/* 103 */             title:item['title'],
/* 104 */             thumb:item['thumb_src'],
/* 105 */             permalink:item['permalink']
/* 106 */          }
/* 107 */       }));
/* 108 */    });
/* 109 */ }
