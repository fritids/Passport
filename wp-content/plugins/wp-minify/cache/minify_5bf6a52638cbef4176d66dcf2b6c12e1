/* shebang.js */

/* 451 */ function roundTo(num, place){
/* 452 */ 	var nn = Math.round(num/place);
/* 453 */ 	return nn * place;
/* 454 */ }
/* 455 */ 
/* 456 */ function addCommas(nStr){
/* 457 */ 	nStr += '';
/* 458 */ 	x = nStr.split('.');
/* 459 */ 	x1 = x[0];
/* 460 */ 	x2 = x.length > 1 ? '.' + x[1] : '';
/* 461 */ 	var rgx = /(\d+)(\d{3})/;
/* 462 */ 	while (rgx.test(x1)) {
/* 463 */ 		x1 = x1.replace(rgx, '$1' + ',' + '$2');
/* 464 */ 	}
/* 465 */ 	return x1 + x2;
/* 466 */ }
/* 467 */ 
/* 468 */ function toNumber(val){
/* 469 */ 	var str = String(val);
/* 470 */ 	str = str.replace(/[^\d.-]/g, "");
/* 471 */ 	var num = Number(str);
/* 472 */ 	return num;
/* 473 */ }
/* 474 */ 
/* 475 */ function toLower(str){
/* 476 */ 	if (str){
/* 477 */ 		return str.toLowerCase();
/* 478 */ 	}
/* 479 */ 	return str;
/* 480 */ }
/* 481 */ 
/* 482 */ function toBoolean(val){
/* 483 */ 	if (val == 0 || val == false || val == '0' || val == 'false' || val == ''){
/* 484 */ 		return false;
/* 485 */ 	} else {
/* 486 */ 		return true;
/* 487 */ 	}
/* 488 */ }
