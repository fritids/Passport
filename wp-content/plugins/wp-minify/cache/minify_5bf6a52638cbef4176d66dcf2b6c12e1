/* 8   */ 		$('#fixed-article-body-nav').localScroll({duration: 1000, onAfter: articleNavigator.resetClicked});
/* 9   */ 	} catch(e){
/* 10  */ 		trace('articleNavigator not found');
/* 11  */ 	}
