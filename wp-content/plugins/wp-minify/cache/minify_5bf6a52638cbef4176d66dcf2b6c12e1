/* ari.js */

/* 1  */ var articleNavigator;
/* 2  */ 
/* 3  */ $(document).ready(function() {
/* 4  */ 	
/* 5  */ 	function ArticleNavigator() {
/* 6  */ 		this.refresh();
/* 7  */ 	}
/* 8  */ 
/* 9  */ 	$('#fixed-article-body-nav').localScroll({duration: 1000, onAfter: this.resetClicked});
/* 10 */ 	var wasClicked = false;
/* 11 */ 	
/* 12 */ 	ArticleNavigator.prototype.resetClicked = function() {
/* 13 */ 		wasClicked = false;
/* 14 */ 	}
/* 15 */ 	
/* 16 */ 	$(".article-body-nav-item").live('click', function() {
/* 17 */ 		if ($(this).hasClass('item-active')){
/* 18 */ 		} else {
/* 19 */ 			wasClicked = true;
/* 20 */ 			$(".article-body-nav").children().removeClass("item-active");
/* 21 */ 			$(this).addClass("item-active");
/* 22 */ 		}
/* 23 */ 	});
/* 24 */ 	
/* 25 */ 	ArticleNavigator.prototype.refresh = function(){
/* 26 */ 		if ($("#fixed-article-body-nav").is('*')) {
/* 27 */ 			// only do this if not ie7
/* 28 */ 			if (window.pageYOffset != undefined) {
/* 29 */ 				slideArray = new Array();
/* 30 */ 				
/* 31 */ 				$('.ui-article-block, tr.excel-output-header-marker').each(function(e) {
/* 32 */ 					var offset = $(this).offset().top - ($(window).height() * 0.2);
/* 33 */ 					var offsetBottom = $(this).height() + offset;
/* 34 */ 					var id = $(this).attr('id');
/* 35 */ 					if (!id){
/* 36 */ 						var newID = $(this).text();
/* 37 */ 						$(this).attr('id', newID);
/* 38 */ 						id = newID;
/* 39 */ 					}
/* 40 */ 					id = '#' + id;
/* 41 */ 					var itemArray = new Array(offset, offsetBottom, id);
/* 42 */ 					slideArray.push(itemArray);
/* 43 */ 				});
/* 44 */ 				
/* 45 */ 				trace(slideArray);
/* 46 */ 				
/* 47 */ 				$(window).scroll(function() {
/* 48 */ 					if (wasClicked == false) {
/* 49 */ 						var scrollPos = $(window).scrollTop();
/* 50 */ 						if (scrollPos == 0) {

/* ari.js */

/* 51 */ 							scrollPos = document.documentElement.scrollTop;
/* 52 */ 						}
/* 53 */ 						var docHeight = $(document).height();
/* 54 */ 						retarget();
/* 55 */ 					}
/* 56 */ 				});
/* 57 */ 			}
/* 58 */ 		}
/* 59 */ 	};
/* 60 */ 	
/* 61 */ 	function retarget() {
/* 62 */ 		var currentIndex;
/* 63 */ 		var scrollPos = $(window).scrollTop();
/* 64 */ 		if (scrollPos == 0) {
/* 65 */ 			scrollPos = document.documentElement.scrollTop;
/* 66 */ 		}
/* 67 */ 		slideSize = slideArray.length;
/* 68 */ 		
/* 69 */ 		// condition for if you are above the top item
/* 70 */ 		if (scrollPos < slideArray[0][0]) {
/* 71 */ 			$('.item-active').removeClass('item-active');
/* 72 */ 			$('.article-body-nav-item:eq(0)').addClass('item-active');
/* 73 */ 			
/* 74 */ 		// condition for if you're on the last item
/* 75 */ 		} else if (scrollPos > slideArray[slideSize-1][0]) {
/* 76 */ 			$('.item-active').removeClass('item-active');
/* 77 */ 			$('.article-body-nav-item:last').addClass('item-active');
/* 78 */ 			
/* 79 */ 		} else {
/* 80 */ 			// find current item's index
/* 81 */ 			for (var i=0; i<slideArray.length; i++) {
/* 82 */ 				if (scrollPos >= slideArray[i][0] && scrollPos < slideArray[i+1][0]) {
/* 83 */ 					trace('Hit on ' + slideArray[i][2]);
/* 84 */ 					currentIndex = i;
/* 85 */ 					break;
/* 86 */ 				}
/* 87 */ 			}
/* 88 */ 			$('.item-active').removeClass('item-active');
/* 89 */ 			$('.article-body-nav-item:eq('+currentIndex+')').addClass('item-active');
/* 90 */ 		}
/* 91 */ 		$('.navlink').show();
/* 92 */ 		//console.log('done');
/* 93 */ 	}
/* 94 */ 	
/* 95 */ 	articleNavigator = new ArticleNavigator();
/* 96 */ });

