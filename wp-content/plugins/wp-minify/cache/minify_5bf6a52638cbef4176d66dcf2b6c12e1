/* 47 */ 		function pad(number, length) {
/* 48 */ 			var str = '' + number;
/* 49 */ 			while (str.length < length) {
/* 50 */ 				str = '0' + str;

/* upstatement-admin.js */

/* 51 */ 			}
/* 52 */ 			return str;
/* 53 */ 		}
/* 54 */ 		
/* 55 */ 		function addGalleryOptions(){
/* 56 */ 			var powerTools = $('<span>Tools: <a href="#" id="autonumber">Auto-Number</a> | <a href="#" id="gal-showall">Show All</a> | <a href="#" id="gal-hideall">Hide All</a></span>');
/* 57 */ 			$('#sort-buttons').prepend(powerTools);
/* 58 */ 			$('#autonumber').click(autoNumberGallery);
/* 59 */ 			$('#gal-showall').click(expandAllGalleryImages);
/* 60 */ 			$('#gal-hideall').click(collapseAllGalleryImages);
/* 61 */ 		}
/* 62 */ 		
